<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          helpPath="en/cq/current/wcm/default_components.html#Text"
          jcr:primaryType="cq:Dialog" title="Inherit for Toolbar"
          xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <inheritTab
                jcr:primaryType="cq:Panel"
                title="Inherit">
            <items jcr:primaryType="cq:WidgetCollection">
                <cancelinheritparent jcr:primaryType="cq:Widget"
                           fieldLabel="Cancel Inherit Parent"
                           allowBlank="{Boolean}true"
                           name="./cancelInheritParent"
                           options="/etc/tags/component-dialog/common/true-false.valuelist.json"
                           type="select"
                           defaultValue=""
                           value=""
                           xtype="selection"/>
            </items>
        </inheritTab>
    </items>
</jcr:root>

