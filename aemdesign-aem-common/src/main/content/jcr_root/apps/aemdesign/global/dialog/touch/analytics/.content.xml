<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Analytics Dialog"
          helpPath=""
          sling:resourceType="cq/gui/components/authoring/dialog"
          extraClientlibs="[]">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
        <layout
                jcr:primaryType="nt:unstructured"
                type="nav"
                sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">
            <analyticsTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Analytics"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                        <basic
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Basic"
                                granite:id="analytics-basic"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <analyticsEventType
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldLabel="Event Type"
                                            name="./analyticsEventType"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-analytics/eventtype"/>
                                    </analyticsEventType>
                                    <analyticsLinkType
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Link Type"
                                            fieldLabel="Link Type"
                                            name="./analyticsLinkType"
                                            required="{Boolean}false"/>
                                    <analyticsLinkLocation
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Link Location"
                                            fieldLabel="Link Location"
                                            name="./analyticsLinkLocation"
                                            required="{Boolean}false"/>
                                    <analyticsLinkDescription
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Link Description"
                                            fieldLabel="Link Description"
                                            name="./analyticsLinkDescription"
                                            required="{Boolean}false"/>

                                </items>
                        </basic>
                        <advanced
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Advanced"
                                    granite:class="hidden"
                                    granite:id="analytics-advanced"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <analyticsType
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldDescription="/content/cq:tags/{tenant}/component-analytics/type"
                                            fieldLabel="Analytics Type"
                                            name="./analyticsType"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-analytics/type"/>
                                    </analyticsType>
                                    <analyticsEventLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Event Label"
                                            name="./analyticsEventLabel"
                                            required="{Boolean}false"/>
                                    <analyticsHitType
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldDescription="/content/cq:tags/{tenant}/component-analytics/hittype"
                                            fieldLabel="Hit Type"
                                            name="./analyticsHitType"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-analytics/hittype"/>
                                    </analyticsHitType>
                                    <analyticsEventCategory
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/common/form/tagfield"
                                            fieldDescription="/content/cq:tags/{tenant}/component-analytics/event-category"
                                            fieldLabel="Event Category"
                                            multiple="{Boolean}true"
                                            rootPath="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-analytics/event-category"
                                            name="./analyticsEventCategory"/>
                                    <analyticsEventAction
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/common/form/tagfield"
                                            fieldDescription="/content/cq:tags/{tenant}/component-analytics/event-action"
                                            fieldLabel="Event Action"
                                            multiple="{Boolean}true"
                                            rootPath="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-analytics/event-action"
                                            name="./analyticsEventAction"/>
                                    <analyticsTransport
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription=""
                                            fieldLabel="Transport [GA]"
                                            name="./analyticsTransport"
                                            required="{Boolean}false"/>
                                    <analyticsNonInteraction
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldDescription="Is action interactive"
                                            fieldLabel="Non Interactive"
                                            name="./analyticsNonInteraction"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"/>
                                    </analyticsNonInteraction>
                                </items>
                            </advanced>

                        </items>
                    </column>
                </items>
            </analyticsTab>
            <pageAnalyticsTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Page Analytics"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Basic"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <note
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/authoring/dialog/note"
                                            note="This information is used only when page is viewed directly"/>

                                    <analyticsPageName
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Page Name"
                                            name="./analyticsPageName"
                                            required="{Boolean}false"/>
                                    <analyticsPageType
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldLabel="Page Type"
                                            name="./analyticsPageType"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="tagname"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-analytics/pagetype"/>
                                    </analyticsPageType>
                                    <analyticsPlatform
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Default value is [aem]"
                                            fieldLabel="Platform"
                                            name="./analyticsPlatform"
                                            required="{Boolean}false"/>
                                    <analyticsAbort
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldLabel="Abort"
                                            name="./analyticsAbort"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"/>
                                    </analyticsAbort>
                                </items>
                            </basic>

                        </items>
                    </column>
                </items>
            </pageAnalyticsTab>
        </items>
    </content>
</jcr:root>
