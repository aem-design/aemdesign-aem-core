<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:rep="internal"
          jcr:primaryType="cq:Dialog"
          helpPath="en/cq/current/wcm/default_components.html#List"
          title="Permissions Configs"
          xtype="tabpanel">

    <items jcr:primaryType="cq:WidgetCollection">



        <permissionCheckTab
                jcr:primaryType="cq:Panel"
                id="permissionCheckTab"
                title="Locked">
            <items jcr:primaryType="cq:WidgetCollection">
                <descriptionLabel
                        jcr:primaryType="cq:Widget"
                        html="&lt;span style=&quot;color: red; font-weight: bold; font-size: large&quot;>You do not have access to update this component, this is locked for Content Admins.&lt;/span>"
                        xtype="label"/>
                <accessCheck
                        jcr:primaryType="cq:Widget"
                        text="If you can see this you have permissions to update this component."
                        name="permissionCheckTabAccessCheck"
                        xtype="label">
                    <rep:policy jcr:primaryType="rep:ACL">
                        <deny
                                jcr:primaryType="rep:DenyACE"
                                rep:principalName="contributor"
                                rep:privileges="{Name}[jcr:read]" />
                    </rep:policy>
                </accessCheck>
            </items>
        </permissionCheckTab>

    </items>

</jcr:root>



