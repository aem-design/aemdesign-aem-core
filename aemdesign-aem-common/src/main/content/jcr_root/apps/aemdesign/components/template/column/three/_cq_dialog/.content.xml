<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <basic
                jcr:primaryType="nt:unstructured"
                jcr:title="Basic"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Title"
                                        name="./jcr:title"/>
                                    <tags
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                        fieldLabel="Tags/Keywords"
                                        multiple="{Boolean}true"
                                        name="./cq:tags"/>
                                    <hideinnav
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldLabel="Hide in Navigation"
                                        name="./hideInNav"
                                        text="Hide in Navigation"
                                        value="true"/>
                                </items>
                            </basic>
                            <more_titles
                                jcr:primaryType="nt:unstructured"
                                jcr:title="More Titles and Description"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <pagetitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Page Title"
                                        name="./pageTitle"/>
                                    <navtitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Navigation Title"
                                        name="./navTitle"/>
                                    <subtitle
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Subtitle"
                                        name="./subtitle"/>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldLabel="Description"
                                        name="./jcr:description"/>
                                </items>
                            </more_titles>
                            <onofftime
                                jcr:primaryType="nt:unstructured"
                                jcr:title="On/Off Time"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <ontime
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                        fieldLabel="On Time"
                                        name="./onTime"
                                        type="datetime"/>
                                    <offtime
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
                                        fieldLabel="Off Time"
                                        name="./offTime"
                                        type="datetime"/>
                                    <onTimeHint
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                        name="./onTime@TypeHint"
                                        value="Date"/>
                                    <offTimeHint
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                        name="./offTime@TypeHint"
                                        value="Date"/>
                                </items>
                            </onofftime>
                            <vanity
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Vanity URL"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <vanityPath
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        fieldLabel="Vanity URL">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            name="./sling:vanityPath"
                                            required="{Boolean}true"/>
                                    </vanityPath>
                                    <vanityPathRedirect
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldLabel="Redirect Vanity URL"
                                        name="./sling:redirect"
                                        text="Redirect Vanity URL"/>
                                </items>
                            </vanity>
                        </items>
                    </column>
                </items>
            </basic>
            <advanced
                jcr:primaryType="nt:unstructured"
                jcr:title="Advanced"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <advanced
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <language
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldLabel="Language"
                                        name="./jcr:language">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </language>
                                    <redirect
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Redirect"
                                        name="./redirectTarget"/>
                                    <designpath
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Design"
                                        name="./cq:designPath"
                                        rootPath="/etc/designs"/>
                                    <alias
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Alias"
                                        name="./sling:alias"/>
                                </items>
                            </advanced>
                            <siteStructure/>
                            <cug
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Closed User Group"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <manageCug/>
                                    <deprecatedCugInfo/>
                                    <deprecatedCug/>
                                </items>
                            </cug>
                            <export
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Export"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <template
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Export Configuration"
                                        name="./cq:exportTemplate"
                                        rootPath="/etc/contentsync/templates"/>
                                </items>
                            </export>
                        </items>
                    </column>
                </items>
            </advanced>
            <image/>
            <livecopy/>
            <blueprint/>
            <cloudservices
                jcr:primaryType="nt:unstructured"
                jcr:title="Cloud Services"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </column>
                </items>
            </cloudservices>
            <styleTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Style"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <componentId
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="ID"
                                name="./componentId"
                                required="{Boolean}false"/>
                            <componentTheme
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Theme"
                                multiple="{Boolean}true"
                                name="./componentTheme"/>
                            <componentModifiers
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Modifiers"
                                multiple="{Boolean}true"
                                name="./componentModifiers"/>
                            <componentModule
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Module"
                                multiple="{Boolean}true"
                                name="./componentModule"/>
                            <componentChevron
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Chevron"
                                multiple="{Boolean}true"
                                name="./componentChevron"/>
                            <componentIcon
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Icon"
                                multiple="{Boolean}true"
                                name="./componentIcon"/>
                            <componentBooleanAttrs
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Boolean Attributes"
                                multiple="{Boolean}true"
                                name="./componentBooleanAttrs"/>
                            <position
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Position coordinates"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <positions
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/sizefield"
                                        fieldDescription="The X and Y values of the Position"
                                        fieldLabel="Positions">
                                        <width
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                            name="./positionX"
                                            unit="X position"/>
                                        <height
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                            name="./positionY"
                                            unit="Y position"/>
                                    </positions>
                                </items>
                            </position>
                            <animation
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Animation"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <animationConfigGroupLabel/>
                                    <animationEnabled
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="Should the animation be enabled for this component?"
                                        fieldLabel="Enable animations"
                                        name="./animationEnabled"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationEnabled>
                                    <animationName
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="The type of animation to run"
                                        fieldLabel="Name"
                                        name="./animationName"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationName>
                                    <animationOnce
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="Should the animation only run once? By default is runs everytime you scroll past the element on the page"
                                        fieldLabel="Run Once?"
                                        name="./animationOnce"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationOnce>
                                    <animationEasing
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="How should the animation play?"
                                        fieldLabel="Easing"
                                        name="./animationEasing"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationEasing>
                                    <animationDelay
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        defaultValue=""
                                        fieldDescription="How many milliseconds should occur before the animation runs?"
                                        fieldLabel="Delay"
                                        name="./animationDelay"
                                        required="{Boolean}false"
                                        step="100"/>
                                    <animationDuration
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        defaultValue=""
                                        fieldDescription="How long should the animation run for? Minimum is '400' and the maximum is '3000'. Values must be in increments of 50, i.e. 450, 500, 550..."
                                        fieldLabel="Duration"
                                        name="./animationDuration"
                                        required="{Boolean}false"
                                        step="50"/>
                                </items>
                            </animation>
                        </items>
                    </column>
                </items>
            </styleTab>
            <siteThemeTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Theme"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <siteThemeCategory
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Site theme Category to include"
                                fieldLabel="Site Theme"
                                name="./siteThemeCategory"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </siteThemeCategory>
                            <siteThemeColor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Used by browsers for Tab and Browser windows colors"
                                fieldLabel="Theme Color"
                                name="./siteThemeColor"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </siteThemeColor>
                            <siteTileColor
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Used by browsers for Tab and Browser tile colors"
                                fieldLabel="Tile Color"
                                name="./siteTileColor"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </siteTileColor>
                        </items>
                    </column>
                </items>
            </siteThemeTab>
            <accessibilityTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Accessibility"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <ariaLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Defines a string value that labels the current element."
                                fieldLabel="Aria Label"
                                name="./ariaLabel"
                                required="{Boolean}false"/>
                            <ariaRole
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Main indicator of type. This semantic association allows tools to present and support interaction with the object in a manner that is consistent with user expectations about other objects of that type."
                                fieldLabel="Aria Role"
                                name="./ariaRole"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaRole>
                            <ariaDescribedBy
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) that describes the object."
                                fieldLabel="Aria Described By"
                                name="./ariaDescribedBy"
                                required="{Boolean}false"/>
                            <ariaLabelledBy
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) that labels the current element."
                                fieldLabel="Aria Labelled By"
                                name="./ariaLabelledBy"
                                required="{Boolean}false"/>
                            <ariaControls
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) whose contents or presence are controlled by the current element. "
                                fieldLabel="Aria Controls"
                                name="./ariaControls"
                                required="{Boolean}false"/>
                            <ariaLive
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Indicates that an element will be updated, and describes the types of updates the user agents, assistive technologies, and user can expect from the live region."
                                fieldLabel="Aria Live"
                                name="./ariaLive"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaLive>
                            <ariaHidden
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Indicates that the element and all of its descendants are not visible or perceivable to any user as implemented by the author."
                                fieldLabel="Aria Hidden"
                                name="./ariaHidden"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaHidden>
                            <ariaHaspopup
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Indicates that the element has a popup context menu or sub-level menu."
                                fieldLabel="Aria Has-Popup"
                                name="./ariaHaspopup"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaHaspopup>
                            <ariaAccessKey
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) whose contents or presence are controlled by the current element. "
                                fieldLabel="Access Key"
                                name="./ariaAccessKey"
                                required="{Boolean}false"/>
                        </items>
                    </column>
                </items>
            </accessibilityTab>
            <openGraphTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Metadata"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <contentType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Used to include specific content metadata"
                                fieldLabel="OG Content Type"
                                name="./metadataContentType"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </contentType>
                        </items>
                    </column>
                </items>
            </openGraphTab>
        </items>
    </content>
</jcr:root>
