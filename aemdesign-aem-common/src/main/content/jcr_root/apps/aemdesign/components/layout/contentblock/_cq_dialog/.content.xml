<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Content Block"
          helpPath=""
          sling:resourceType="cq/gui/components/authoring/dialog"
          extraClientlibs="[]">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
        <layout
                jcr:primaryType="nt:unstructured"
                type="nav"
                sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">

            <basicTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Basic"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Basic"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">

                                    <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Title"
                                            name="./title"/>

                                    <titleType
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldDescription="which paragraph tag to use for Title display"
                                            fieldLabel="Title Type"
                                            name="./titleType"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/etc/tags/component-dialog/html/heading"/>
                                    </titleType>

                                    <hideTitle
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldLabel="Hide Title"
                                            name="./hideTitle"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/etc/tags/component-dialog/common/true-false"/>
                                    </hideTitle>
                                    <hideTopLink
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldDescription="will hide link Back to Top at the bottom of section"
                                            fieldLabel="Hide Back to Top link"
                                            name="./hideTopLink"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/etc/tags/component-dialog/common/true-false"/>
                                    </hideTopLink>
                                    <hideInContentMenu
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                            fieldDescription="will hide this section from Content Block List"
                                            fieldLabel="Hide Title in content block menu"
                                            name="./hideInMenu"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/etc/tags/component-dialog/common/true-false"/>
                                    </hideInContentMenu>
                                </items>
                            </basic>
                        </items>
                    </column>
                </items>
            </basicTab>


            <layoutTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Layout"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Layout"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <variant
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldLabel="Variant"
                                            name="./variant"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Default"
                                                    value=""/>
                                            <container
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Container"
                                                    value="container"/>
                                            <containerVideo
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Container with Video Background"
                                                    value="containerVideo"/>
                                            <floating
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Floating Section"
                                                    value="floating"/>
                                            <advsection
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Advanced Section"
                                                    value="advsection"/>
                                            <fieldset
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Field Set in Form"
                                                    value="fieldset"/>
                                            <descriptionlist
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Description List"
                                                    value="descriptionlist"/>
                                        </items>
                                    </variant>
                                </items>
                            </variant>
                        </items>
                    </column>
                </items>
            </layoutTab>

            <linkLeftTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Left Links"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Left Links"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="will appear on left of links"
                                            fieldLabel="Title"
                                            name="./linksLeftTitle"/>
                                    <linksLeft
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                            fieldDescription="These links will appear before links"
                                            fieldLabel="Left Links">
                                        <field
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                name="./linkPagesLeft"
                                                rootPath="/content"
                                                required="{Boolean}false"/>
                                    </linksLeft>
                                </items>
                            </variant>
                        </items>
                    </column>
                </items>
            </linkLeftTab>

            <linkRightTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Right Links"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Right Links"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="These links will appear before links"
                                            fieldLabel="Title"
                                            name="./linksRightTitle"/>
                                    <linksRight
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                            fieldDescription="These links will appear on the right title"
                                            fieldLabel="Right Links">
                                        <field
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                name="./linkPagesRight"
                                                rootPath="/content"
                                                required="{Boolean}false"/>
                                    </linksRight>
                                </items>
                            </variant>
                        </items>
                    </column>
                </items>
            </linkRightTab>


            <imageBackgroundTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Background Image"
                    path="/apps/aemdesign/global/dialog/touch/asset/content/items/imageBackgroundTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>

            <videoBackgroundTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Background Video"
                    path="/apps/aemdesign/global/dialog/touch/asset/content/items/videoBackgroundTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>

            <styleTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Style"
                    path="/apps/aemdesign/global/dialog/touch/style/content/items/styleTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
            <accessibilityTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Accessibility"
                    path="/apps/aemdesign/global/dialog/touch/accessibility/content/items/accessibilityTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
            <analyticsTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Analytics"
                    path="/apps/aemdesign/global/dialog/touch/analytics/content/items/analyticsTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
        </items>
    </content>
</jcr:root>
