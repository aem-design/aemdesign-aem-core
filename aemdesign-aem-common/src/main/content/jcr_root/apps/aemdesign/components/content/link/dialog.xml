<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Dialog"
          helpPath="en/cq/current/wcm/default_components.html#Text"
          description="Call to Acton can be a Link, Button etc"
          title="Link"
          xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tabs jcr:primaryType="cq:TabPanel">
            <items jcr:primaryType="cq:WidgetCollection">
                <contentTab
                        jcr:primaryType="cq:Widget"
                        title="Attributes">
                    <items jcr:primaryType="cq:WidgetCollection">

                        <label
                                jcr:primaryType="cq:Widget"
                                path="/apps/aemdesign/global/dialog/classic/rteprofile/items/link/items/label.infinity.json"
                                xtype="cqinclude"/>
                        <linkUrl
                                jcr:primaryType="cq:Widget"
                                path="/apps/aemdesign/global/dialog/classic/common/items/linkTab/items/linkUrl.infinity.json"
                                xtype="cqinclude"/>
                        <hrefTarget
                                jcr:primaryType="cq:Widget"
                                path="/apps/aemdesign/global/dialog/classic/common/items/linkTab/items/linkTarget.infinity.json"
                                xtype="cqinclude"/>
                        <variant
                                jcr:primaryType="cq:Widget"
                                defaultValue=""
                                fieldLabel="Variant"
                                name="./variant"
                                allowBlank="{Boolean}true"
                                value=""
                                type="select"
                                xtype="selection">
                            <options jcr:primaryType="cq:WidgetCollection">
                                <default
                                        jcr:primaryType="nt:unstructured"
                                        value=""
                                        text="Default"/>
                                <button
                                        jcr:primaryType="nt:unstructured"
                                        value="button"
                                        text="Button"/>
                            </options>
                        </variant>
                    </items>
                </contentTab>
                <styleTab
                    jcr:primaryType="cq:Widget"
                    path="/apps/aemdesign/global/dialog/classic/style/items/common.infinity.json"
                    xtype="cqinclude"/>
                <accessibilityTab jcr:primaryType="cq:Panel"
                    path="/apps/aemdesign/global/dialog/classic/accessibility/items/common.infinity.json"
                    xtype="cqinclude" />
                <dataAttributesTab jcr:primaryType="cq:Panel"
                    path="/apps/aemdesign/global/dialog/classic/attributes/items/common.infinity.json"
                    xtype="cqinclude" />
                <analyticsTab jcr:primaryType="cq:Panel"
                    path="/apps/aemdesign/global/dialog/classic/analytics/items/common.infinity.json"
                    xtype="cqinclude" />
            </items>
        </tabs>
    </items>
</jcr:root>