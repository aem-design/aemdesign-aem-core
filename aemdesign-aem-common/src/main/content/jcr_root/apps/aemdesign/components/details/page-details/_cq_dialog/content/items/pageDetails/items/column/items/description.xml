                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="p3"
                    text="Paragraph (Level 3)"/>
                <textPrimary
                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="text-primary"
                    text="Colour (Primary)"/>
                <p2
                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="p2"
                    text="Paragraph (Level 2)"/>
                <textInfo
                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="text-info"
                    text="Colour (Info)"/>
                <houschkaFontFamilyBold jcr:primaryType="nt:folder"/>
                <h2 jcr:primaryType="nt:folder"/>
                <h5 jcr:primaryType="nt:folder"/>
                <h4 jcr:primaryType="nt:folder"/>
                <iconMedium
                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="icon-md"
                    text="Icon Size (Medium)"/>
                <h3 jcr:primaryType="nt:folder"/>
                <iconLarge
                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="icon-lg"
                    text="Icon Size (Large)"/>
                <textDanger
                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="text-danger"
                    text="Colour (Danger)"/>
                <houschkaFontFamilyExtraBold jcr:primaryType="nt:folder"/>
                <textWarning
                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="text-warning"
                    text="Colour (Warning)"/>
                <p1
                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="p1"
                    text="Paragraph (Level 1)"/>
                <h6 jcr:primaryType="nt:folder"/>
                <h1 jcr:primaryType="nt:folder"/>
                <iconExtraLarge
                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="icon-xl"
                    text="Icon Size (Extra Large)"/>
                <textWhite
                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="text-white"
                    text="Colour (White)"/>
                <textMuted
                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="text-muted"
                    text="Colour (Muted)"/>
                <textSuccess
                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="text-success"
                    text="Colour (Success)"/>
                <houschkaFontFamilyLight jcr:primaryType="nt:folder"/>
                <iconExtraSmall
                    jcr:primaryType="cq:Tag"
                    sling:resourceType="cq/tagging/components/tag"
                    cssName="icon-xs"
                    text="Icon Size (Extra Small)"/>
                <houschkaFontFamilyMedium jcr:primaryType="nt:folder"/>
            </styles>
        </styles>
