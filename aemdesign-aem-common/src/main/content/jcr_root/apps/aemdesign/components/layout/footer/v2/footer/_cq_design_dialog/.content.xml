<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Footer (Design)"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <plugins
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">


                    <inherit
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Inherit"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                        jcr:primaryType="nt:unstructured"
                                        granite:class="foundation-layout-util-maximized-alt"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <inherit
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Inherit"
                                                granite:class="js-cq-IPEPlugin-group"
                                                path="/apps/aemdesign/global/dialog/touch/inherit/content/items/inheritTab/items/column/items/inherit"
                                                resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </inherit>


                    <layout
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Layout"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                        jcr:primaryType="nt:unstructured"
                                        granite:class="foundation-layout-util-maximized-alt"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <layout
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Layout"
                                                granite:class="js-cq-IPEPlugin-group"
                                                path="/apps/aemdesign/components/layout/footer/v2/footer/cq:dialog/content/items/layoutTab"
                                                resourceType="granite/ui/components/coral/foundation/container"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </layout>


                    <imageBackgroundTab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Background Image"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                        jcr:primaryType="nt:unstructured"
                                        granite:class="foundation-layout-util-maximized-alt"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <imageBackgroundTab
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Background Image"
                                                granite:class="js-cq-IPEPlugin-group"
                                                path="/apps/aemdesign/global/dialog/touch/asset_design/content/items/fixedcol/items/col1/items/imageBackgroundTab"
                                                resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </imageBackgroundTab>


                    <style
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Style"
                        path="/apps/aemdesign/global/dialog/touch/style_design/content"
                        resourceType="granite/ui/components/coral/foundation/container"
                        sling:resourceType="granite/ui/components/coral/foundation/include"/>
                    <accessibility
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Accessibility"
                        path="/apps/aemdesign/global/dialog/touch/accessibility_design/content"
                        resourceType="granite/ui/components/coral/foundation/container"
                        sling:resourceType="granite/ui/components/coral/foundation/include"/>
                </items>
            </plugins>
        </items>
    </content>
</jcr:root>
