<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Facebook Like Box (Design)"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <plugins
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <layout
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Layout"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                            jcr:primaryType="nt:unstructured"
                                            granite:class="foundation-layout-util-maximized-alt"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Layout"
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    path="/apps/aemdesign/components/widgets/facebook/likebox/cq:dialog/content/items/layoutTab"
                                                    resourceType="granite/ui/components/coral/foundation/container"
                                                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </layout>
                    <advancedStyle
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Advanced CSS"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                            jcr:primaryType="nt:unstructured"
                                            granite:class="foundation-layout-util-maximized-alt"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Advanced CSS"
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    path="/apps/aemdesign/components/widgets/facebook/likebox/cq:dialog/content/items/advancedStyleTab"
                                                    resourceType="granite/ui/components/coral/foundation/container"
                                                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </advancedStyle>


                    <features
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Features"
                            path="/apps/aemdesign/global/dialog/touch/text_design/features"
                            resourceType="granite/ui/components/coral/foundation/container"
                            sling:resourceType="granite/ui/components/coral/foundation/include"/>
                    <formatting
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Formatting"
                            path="/apps/aemdesign/global/dialog/touch/text_design/formatting"
                            resourceType="granite/ui/components/coral/foundation/container"
                            sling:resourceType="granite/ui/components/coral/foundation/include"/>
                    <paraformat
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Paraformat"
                            path="/apps/aemdesign/global/dialog/touch/text_design/paraformat"
                            resourceType="granite/ui/components/coral/foundation/container"
                            sling:resourceType="granite/ui/components/coral/foundation/include"/>
                    <characters
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Characters"
                            path="/apps/aemdesign/global/dialog/touch/text_design/characters"
                            resourceType="granite/ui/components/coral/foundation/container"
                            sling:resourceType="granite/ui/components/coral/foundation/include"/>


                    <imageConfigTab
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Image Config"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                    maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                            jcr:primaryType="nt:unstructured"
                                            granite:class="foundation-layout-util-maximized-alt"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <imageTab
                                                    jcr:primaryType="nt:unstructured"
                                                    jcr:title="Image Config"
                                                    granite:class="js-cq-IPEPlugin-group"
                                                    path="/apps/aemdesign/components/media/image/cq:dialog/content/items/layoutTab/items/column2/items/imageConfig/items/imageConfig"
                                                    resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </imageConfigTab>


                    <style
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Style"
                            path="/apps/aemdesign/global/dialog/touch/style_design/content"
                            resourceType="granite/ui/components/coral/foundation/container"
                            sling:resourceType="granite/ui/components/coral/foundation/include"/>
                    <accessibility
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Accessibility"
                            path="/apps/aemdesign/global/dialog/touch/accessibility_design/content"
                            resourceType="granite/ui/components/coral/foundation/container"
                            sling:resourceType="granite/ui/components/coral/foundation/include"/>
                </items>
            </plugins>
        </items>
    </content>
</jcr:root>

