<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Dialog"
          helpPath="en/cq/current/wcm/default_components.html#List"
          title="News List"
          selectTab="function(tabToSelect) { WKCD.author.components.list.manageTabs(this.findByType('tabpanel')[0],tabToSelect,false); }"
          hideTab="function(tabToHide) { WKCD.author.components.list.hideTab(tabToHide, this.findByType('tabpanel')[0]); }"
          xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tabs jcr:primaryType="cq:TabPanel"
              xtype="tabpanel">
            <items jcr:primaryType="cq:WidgetCollection">
                <listTab
                        jcr:primaryType="cq:Widget"
                        title="News List"
                        xtype="panel">
                    <items jcr:primaryType="cq:WidgetCollection">
                        <listFrom jcr:primaryType="cq:Widget"
                                  path="/apps/aemdesign/components/lists/list/dialog/items/tabs/items/listTab/items/listFrom.infinity.json"
                                  xtype="cqinclude"/>
                        <orderBy jcr:primaryType="cq:Widget"
                                 path="/apps/aemdesign/components/lists/list/dialog/items/tabs/items/listTab/items/orderBy.infinity.json"
                                 xtype="cqinclude"/>
                        <limit jcr:primaryType="cq:Widget"
                               path="/apps/aemdesign/components/lists/list/dialog/items/tabs/items/listTab/items/limit.infinity.json"
                               xtype="cqinclude"/>
                        <pagination jcr:primaryType="cq:Widget"
                                    path="/apps/aemdesign/components/lists/list/dialog/items/tabs/items/listTab/items/pagination.infinity.json"
                                    xtype="cqinclude"/>
                        <paginationType
                                jcr:primaryType="cq:Widget"
                                defaultValue=""
                                value=""
                                allowBlank="{Boolean}true"
                                fieldLabel="Pagination Type"
                                name="./paginationType"
                                type="select"
                                xtype="selection">
                            <options jcr:primaryType="cq:WidgetCollection">
                                <default
                                        jcr:primaryType="nt:unstructured"
                                        text="Default"
                                        value=""/>
                                <loadMore
                                        jcr:primaryType="nt:unstructured"
                                        value="loadMorePages"
                                        text="Load More"/>
                            </options>
                        </paginationType>

                        <listType jcr:primaryType="cq:Widget"
                                  defaultValue="list"
                                  value="list"
                                  allowBlank="{Boolean}true"
                                  fieldLabel="List Type"
                                  name="./listType"
                                  xtype="hidden">
                        </listType>
                        <detailsBadge jcr:primaryType="cq:Widget"
                                      defaultValue=""
                                      value=""
                                      allowBlank="{Boolean}true"
                                      fieldLabel="Badges"
                                      fieldDescription="Which badge to use from Details component"
                                      name="./detailsBadge"
                                      type="select"
                                      xtype="selection">
                            <options jcr:primaryType="cq:WidgetCollection">
                                <default
                                        jcr:primaryType="nt:unstructured"
                                        value=""
                                        text="Default"/>
                                <categoryTitlePublishDate
                                        jcr:primaryType="nt:unstructured"
                                        value="categoryTitlePublishDate"
                                        text="Category Title Publish Date"/>
                                <imageTitleDescriptionCategory
                                        jcr:primaryType="nt:unstructured"
                                        value="imageTitleDescriptionCategory"
                                        text="Image Title Description Category (Masonry)"/>
                                <imageTitleDescriptionCategoryGrid
                                        jcr:primaryType="nt:unstructured"
                                        value="imageTitleDescriptionCategoryGrid"
                                        text="Image Title Description Category"/>
                            </options>
                        </detailsBadge>
                    </items>
                </listTab>
                <childrenTab jcr:primaryType="cq:Widget"
                              path="/apps/aemdesign/components/lists/list/dialog/items/tabs/items/childrenTab.infinity.json"
                              xtype="cqinclude"/>
                <descendantsTab
                        jcr:primaryType="cq:Widget"
                        path="/apps/aemdesign/components/lists/list/dialog/items/tabs/items/descendantsTab.infinity.json"
                        xtype="cqinclude"/>
                <staticTab
                        jcr:primaryType="cq:Widget"
                        path="/apps/aemdesign/components/lists/list/dialog/items/tabs/items/staticTab.infinity.json"
                        xtype="cqinclude"/>
                <tagsTab
                        jcr:primaryType="cq:Widget"
                        path="/apps/aemdesign/components/lists/list/dialog/items/tabs/items/tagsTab.infinity.json"
                        xtype="cqinclude"/>
                <searchTab
                        jcr:primaryType="cq:Widget"
                        path="/apps/aemdesign/components/lists/list/dialog/items/tabs/items/searchTab.infinity.json"
                        xtype="cqinclude"/>
                <querybuilderTab
                        jcr:primaryType="cq:Widget"
                        path="/apps/aemdesign/components/lists/list/dialog/items/tabs/items/querybuilderTab.infinity.json"
                        xtype="cqinclude"/>
                <layoutTab
                        jcr:primaryType="cq:Panel"
                        title="Layout">

                    <items jcr:primaryType="cq:WidgetCollection">
                        <!--
                        <title
                                jcr:primaryType="cq:Widget"
                                fieldLabel="CSS for Title"
                                name="./cssClassTitle"
                                xtype="textfield"/>
                        <titleLink
                                jcr:primaryType="cq:Widget"
                                fieldLabel="CSS for Title Link"
                                name="./cssClassTitleLink"
                                xtype="textfield"/>-->

                        <variant
                                jcr:primaryType="cq:Widget"
                                defaultValue=""
                                value=""
                                allowBlank="{Boolean}true"
                                fieldLabel="Variant"
                                name="./variant"
                                type="select"
                                xtype="selection">
                            <options jcr:primaryType="cq:WidgetCollection">
                                <default
                                        jcr:primaryType="nt:unstructured"
                                        value=""
                                        text="Default"/>
                                <masonry
                                        jcr:primaryType="nt:unstructured"
                                        value="masonry"
                                        text="Masonry"/>
                            </options>
                        </variant>
                        <printStructure jcr:primaryType="cq:Widget"
                                        fieldLabel="Print List Structure"
                                        fieldDescription="Don't print structure of the list, omit (OL/UL and LI tags)"
                                        allowBlank="{Boolean}true"
                                        name="./printStructure"
                                        options="/etc/tags/component-dialog/common/yes-no.valuelist.json"
                                        type="select"
                                        defaultValue=""
                                        value=""
                                        xtype="selection"/>
                        <listcontainer
                                jcr:primaryType="cq:Widget"
                                fieldLabel="CSS for List Container"
                                name="./cssClassListContainer"
                                xtype="textfield"/>
                        <list
                                jcr:primaryType="cq:Widget"
                                fieldLabel="CSS for List"
                                name="./cssClassList"
                                xtype="textfield"/>
                        <item
                                jcr:primaryType="cq:Widget"
                                fieldLabel="CSS for Item"
                                name="./cssClassItem"
                                xtype="textfield"/>
                        <itemlink
                                jcr:primaryType="cq:Widget"
                                fieldLabel="CSS for Item Link"
                                name="./cssClassItemLink"
                                xtype="textfield"/>
                        <!--
                        <pageinationcontainer
                                jcr:primaryType="cq:Widget"
                                fieldLabel="CSS for Pagination Container"
                                name="./cssClassPaginationContainer"
                                xtype="textfield"/>-->
                    </items>
                </layoutTab>
                <styleTab
                        jcr:primaryType="cq:Widget"
                        path="/apps/aemdesign/global/dialog/classic/common/items/styleTab.infinity.json"
                        xtype="cqinclude"/>
            </items>
        </tabs>
    </items>
</jcr:root>
