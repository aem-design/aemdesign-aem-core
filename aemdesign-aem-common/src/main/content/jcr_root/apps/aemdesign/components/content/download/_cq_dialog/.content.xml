<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Download"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="en/cq/current/wcm/default_components.html#Download">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <downloadTab
                jcr:primaryType="nt:unstructured"
                jcr:title="File Download"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <file/>
                        </items>
                    </column>
                </items>
            </downloadTab>
            <layoutTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Layout"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Layout"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <variant
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Variant"
                                        name="./variant"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                jcr:primaryType="nt:unstructured"
                                                text="Default"
                                                value=""/>
                                            <simple
                                                jcr:primaryType="nt:unstructured"
                                                text="Simple"
                                                value="simple"/>
                                            <card
                                                jcr:primaryType="nt:unstructured"
                                                text="Card"
                                                value="card"/>
                                        </items>
                                    </variant>
                                </items>
                            </variant>
                            <info
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Info"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Set image title, does not apply to Licensed Assets"
                                        fieldLabel="Title"
                                        name="./title"/>
                                    <titleType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="which paragraph tag to use for Title display"
                                        fieldLabel="Title Type"
                                        name="./titleType"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </titleType>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldDescription="Set image description, does not apply to Licensed Assets"
                                        fieldLabel="Description"
                                        name="./description"/>
                                </items>
                            </info>
                            <thumbnailStyle
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Thumbnail"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <thumbnail
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="Pick type of thumbnail to use for download. List in order of Custom Thumbnail Rendition will generate a new thumbnail image."
                                        fieldLabel="Thumbnail Type"
                                        name="./thumbnailType"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                jcr:primaryType="nt:unstructured"
                                                text="Default"
                                                value=""/>
                                            <icon
                                                jcr:primaryType="nt:unstructured"
                                                text="Icon"
                                                value="icon"/>
                                            <dam
                                                jcr:primaryType="nt:unstructured"
                                                text="Asset DAM Rendition"
                                                value="dam"/>
                                            <customdam
                                                jcr:primaryType="nt:unstructured"
                                                text="Thumbnail DAM Rendition"
                                                value="customdam"/>
                                            <custom
                                                jcr:primaryType="nt:unstructured"
                                                text="Custom Thumbnail Rendition"
                                                value="custom"/>
                                        </items>
                                    </thumbnail>
                                    <size
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/sizefield"
                                        fieldDescription="blank values are not added as attributes"
                                        fieldLabel="Thumbnail Size (w,h)">
                                        <width
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                            name="./thumbnailWidth"/>
                                        <height
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                            name="./thumbnailHeight"/>
                                    </size>
                                </items>
                            </thumbnailStyle>
                        </items>
                    </column>
                </items>
            </layoutTab>
            <imageThumbnailTab/>
            <imageThumbnailTabResourceType
                jcr:primaryType="nt:unstructured"
                jcr:title="Thumbnail Image Metadata"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <imageBackgroundTabResourceType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                name="./thumbnail/sling:resourceType"
                                value="aemdesign/components/media/image"/>
                        </items>
                    </column>
                </items>
            </imageThumbnailTabResourceType>
            <styleTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Style"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <componentId
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="ID"
                                name="./componentId"
                                required="{Boolean}false"/>
                            <componentTheme
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Theme"
                                multiple="{Boolean}true"
                                name="./componentTheme"/>
                            <componentModifiers
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Modifiers"
                                multiple="{Boolean}true"
                                name="./componentModifiers"/>
                            <componentModule
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Module"
                                multiple="{Boolean}true"
                                name="./componentModule"/>
                            <componentChevron
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Chevron"
                                multiple="{Boolean}true"
                                name="./componentChevron"/>
                            <componentIcon
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Icon"
                                multiple="{Boolean}true"
                                name="./componentIcon"/>
                            <componentBooleanAttrs
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Boolean Attributes"
                                multiple="{Boolean}true"
                                name="./componentBooleanAttrs"/>
                            <position
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Position coordinates"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <positions
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/sizefield"
                                        fieldDescription="The X and Y values of the Position"
                                        fieldLabel="Positions">
                                        <width
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                            name="./positionX"
                                            unit="X position"/>
                                        <height
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                            name="./positionY"
                                            unit="Y position"/>
                                    </positions>
                                </items>
                            </position>
                            <animation
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Animation"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <animationConfigGroupLabel/>
                                    <animationEnabled
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="Should the animation be enabled for this component?"
                                        fieldLabel="Enable animations"
                                        name="./animationEnabled"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationEnabled>
                                    <animationName
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="The type of animation to run"
                                        fieldLabel="Name"
                                        name="./animationName"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationName>
                                    <animationOnce
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="Should the animation only run once? By default is runs everytime you scroll past the element on the page"
                                        fieldLabel="Run Once?"
                                        name="./animationOnce"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationOnce>
                                    <animationEasing
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="How should the animation play?"
                                        fieldLabel="Easing"
                                        name="./animationEasing"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationEasing>
                                    <animationDelay
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        defaultValue=""
                                        fieldDescription="How many milliseconds should occur before the animation runs?"
                                        fieldLabel="Delay"
                                        name="./animationDelay"
                                        required="{Boolean}false"
                                        step="100"/>
                                    <animationDuration
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        defaultValue=""
                                        fieldDescription="How long should the animation run for? Minimum is '400' and the maximum is '3000'. Values must be in increments of 50, i.e. 450, 500, 550..."
                                        fieldLabel="Duration"
                                        name="./animationDuration"
                                        required="{Boolean}false"
                                        step="50"/>
                                </items>
                            </animation>
                        </items>
                    </column>
                </items>
            </styleTab>
            <accessibilityTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Accessibility"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <ariaLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Defines a string value that labels the current element."
                                fieldLabel="Aria Label"
                                name="./ariaLabel"
                                required="{Boolean}false"/>
                            <ariaRole
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldDescription="Main indicator of type. This semantic association allows tools to present and support interaction with the object in a manner that is consistent with user expectations about other objects of that type."
                                fieldLabel="Aria Role"
                                name="./ariaRole"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaRole>
                            <ariaDescribedBy
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) that describes the object."
                                fieldLabel="Aria Described By"
                                name="./ariaDescribedBy"
                                required="{Boolean}false"/>
                            <ariaLabelledBy
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) that labels the current element."
                                fieldLabel="Aria Labelled By"
                                name="./ariaLabelledBy"
                                required="{Boolean}false"/>
                            <ariaControls
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) whose contents or presence are controlled by the current element. "
                                fieldLabel="Aria Controls"
                                name="./ariaControls"
                                required="{Boolean}false"/>
                            <ariaLive
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldDescription="Indicates that an element will be updated, and describes the types of updates the user agents, assistive technologies, and user can expect from the live region."
                                fieldLabel="Aria Live"
                                name="./ariaLive"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaLive>
                            <ariaHidden
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldDescription="Indicates that the element and all of its descendants are not visible or perceivable to any user as implemented by the author."
                                fieldLabel="Aria Hidden"
                                name="./ariaHidden"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaHidden>
                            <ariaHaspopup
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldDescription="Indicates that the element has a popup context menu or sub-level menu."
                                fieldLabel="Aria Has-Popup"
                                name="./ariaHaspopup"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaHaspopup>
                            <ariaAccessKey
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) whose contents or presence are controlled by the current element. "
                                fieldLabel="Access Key"
                                name="./ariaAccessKey"
                                required="{Boolean}false"/>
                        </items>
                    </column>
                </items>
            </accessibilityTab>
            <analyticsTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Analytics"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <analyticsType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldLabel="Analytics Type"
                                name="./analyticsType"
                                value="">
                                <items jcr:primaryType="nt:unstructured">
                                    <default
                                        jcr:primaryType="nt:unstructured"
                                        text="Default"
                                        value=""/>
                                    <ga
                                        jcr:primaryType="nt:unstructured"
                                        text="Google Analytics"
                                        value="ga"/>
                                    <aa
                                        jcr:primaryType="nt:unstructured"
                                        text="Adobe Analytics"
                                        value="aa"/>
                                </items>
                            </analyticsType>
                            <analyticsHitType
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldDescription="Specifies the event category [AA,GA]"
                                fieldLabel="Hit Type"
                                name="./analyticsHitType"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </analyticsHitType>
                            <analyticsEventCategory
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldDescription="Specifies the event category [AA,GA]"
                                fieldLabel="Event Category"
                                multiple="{Boolean}true"
                                name="./analyticsEventCategory"/>
                            <analyticsEventAction
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldDescription="Specifies the event action [GA]"
                                fieldLabel="Event Action"
                                multiple="{Boolean}true"
                                name="./analyticsEventAction"/>
                            <analyticsEventLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Specifies the event label [AA,GA]"
                                fieldLabel="Event Label"
                                name="./analyticsEventLabel"
                                required="{Boolean}false"/>
                            <analyticsTransport
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription=""
                                fieldLabel="Transport [GA]"
                                name="./analyticsTransport"
                                required="{Boolean}false"/>
                            <analyticsNonInteraction
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldDescription="Is action interactive [GA]"
                                fieldLabel="Non Interactive"
                                name="./analyticsNonInteraction"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </analyticsNonInteraction>
                        </items>
                    </column>
                </items>
            </analyticsTab>
        </items>
    </content>
</jcr:root>
