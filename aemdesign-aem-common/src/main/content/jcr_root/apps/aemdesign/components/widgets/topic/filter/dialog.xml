<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:primaryType="cq:Dialog"
          title="Topic Filter"
          xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tabs jcr:primaryType="cq:TabPanel">
            <items jcr:primaryType="cq:WidgetCollection">
                <filterTab jcr:primaryType="cq:Widget"
                          title="Filter"
                          xtype="panel">
                    <items jcr:primaryType="cq:WidgetCollection">
                        <topic jcr:primaryType="cq:Widget"
                            fieldLabel="Topic Queue"
                            fieldDescription="Notify the subscribers to the filter value is updated"
                            allowBlank="{Boolean}false"
                            name="./topicQueue"
                            options="/etc/tags/component-topic/topic.valuelist.json"
                            type="select"
                            defaultValue=""
                            value=""
                            xtype="selection"/>
                        <filterList jcr:primaryType="cq:Widget"
                            allowBlank="{Boolean}true"
                            defaultValue=""
                            value=""
                            fieldLabel="Filter List"
                            name="./filterList"
                            namespaces="[aemdesign]"
                            xtype="tags"/>
                        <defaultFilters jcr:primaryType="cq:Widget"
                                    allowBlank="{Boolean}true"
                                    defaultValue=""
                                    value=""
                                    fieldLabel="Default Filter"
                                    name="./defaultFilters"
                                    namespaces="[aemdesign]"
                                    xtype="tags"/>
                        <variant
                                jcr:primaryType="cq:Widget"
                                defaultValue=""
                                value=""
                                allowBlank="{Boolean}true"
                                fieldLabel="Variant"
                                name="./variant"
                                type="select"
                                xtype="selection">
                            <options jcr:primaryType="cq:WidgetCollection">
                                <default
                                        jcr:primaryType="nt:unstructured"
                                        value=""
                                        text="Default"/>
                            </options>
                        </variant>
                    </items>
                </filterTab>
                <styleTab
                        jcr:primaryType="cq:Widget"
                        path="/apps/aemdesign/global/dialog/classic/common/items/styleTab.infinity.json"
                        xtype="cqinclude" />
                <accessibilityTab jcr:primaryType="cq:Panel"
                                  path="/apps/aemdesign/global/dialog/classic/common/items/accessibilityTab.infinity.json"
                                  xtype="cqinclude"/>
            </items>
        </tabs>
    </items>
</jcr:root>
