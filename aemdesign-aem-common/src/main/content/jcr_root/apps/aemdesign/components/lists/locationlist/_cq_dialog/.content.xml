<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Location List"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
        <layout
                jcr:primaryType="nt:unstructured"
                type="nav"
                sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">


            <listTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="List"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="List"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <listFrom
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            class="cq-dialog-dropdown-showhide"
                                            cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                            fieldLabel="Build list using"
                                            emptyOption="{Boolean}true"
                                            name="./listFrom">
                                        <items jcr:primaryType="nt:unstructured">
                                            <static
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Fixed list"
                                                    value="static"/>
                                        </items>
                                    </listFrom>
                                    <setStatic
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container"
                                            class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                            showhidetargetvalue="static">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/heading"
                                                    class="coral-Heading coral-Heading--4"
                                                    level="{Long}4"
                                                    text="Options for Fixed list"/>
                                            <well
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pages
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                            class="foundation-layout-util-maximized-alt">
                                                        <field
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                                name="./pages"
                                                                rootPath="/content"/>
                                                    </pages>
                                                </items>
                                            </well>
                                        </items>
                                    </setStatic>
                                </items>
                            </basic>
                        </items>
                    </column1>
                </items>
            </listTab>

            <listConfigTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="List Config"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Config"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <printStructure
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldDescription="Don't print structure of the list, omit (OL/UL and LI tags)"
                                            fieldLabel="Print List Structure"
                                            name="./printStructure"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/etc/tags/aemdesign/component-dialog/common/true-false"/>
                                    </printStructure>


                                </items>
                            </basic>

                        </items>
                    </column>
                </items>
            </listConfigTab>


            <layoutTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Layout"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Layout"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <variant
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldLabel="Variant"
                                            name="./variant"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Default"
                                                    value=""/>
                                        </items>
                                    </variant>
                                    <detailsBadge
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldDescription="Which badge to select from the Details component on the page"
                                            fieldLabel="Use Badge"
                                            name="./detailsBadge"
                                            value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Default"
                                                    value="image"/>


                                        </items>
                                    </detailsBadge>
                                    <topic
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldDescription="Subscribers waiting for notification of the filter value is updated"
                                            fieldLabel="Topic Queue"
                                            name="./topicQueue"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/etc/tags/aemdesign/component-topic/topic"/>
                                    </topic>
                                </items>
                            </variant>
                        </items>
                    </column>
                </items>
            </layoutTab>


            <badgeConfigTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Badge Config"
                    path="/apps/aemdesign/global/dialog/touch/badge/content/items/badgeConfigTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>

            <badgeThumbnailTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Badge Thumbnail"
                    path="/apps/aemdesign/global/dialog/touch/asset/content/items/badgeThumbnailTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>


            <styleTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Style"
                    path="/apps/aemdesign/global/dialog/touch/style/content/items/styleTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
            <accessibilityTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Accessibility"
                    path="/apps/aemdesign/global/dialog/touch/accessibility/content/items/accessibilityTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
            <analyticsTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Analytics"
                    path="/apps/aemdesign/global/dialog/touch/analytics/content/items/analyticsTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
        </items>
    </content>

</jcr:root>
