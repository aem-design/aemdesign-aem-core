<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Image (Design)"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <other
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Design"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <content
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <accordion
                                        granite:class="js-cq-IPEPlugin-container"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="aemdesign/components/coral/foundation/accordion"
                                        variant="quiet">
                                        <items jcr:primaryType="nt:unstructured">
                                            <imageTab
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Image"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                maximized="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <fixedcol
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                                        maximized="{Boolean}true">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <col1
                                                                jcr:primaryType="nt:unstructured"
                                                                granite:class="foundation-layout-util-maximized-alt"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <image
                                                                        jcr:primaryType="nt:unstructured"
                                                                        jcr:title="Default Image"
                                                                        granite:class="js-cq-IPEPlugin-group"
                                                                        path="/apps/aemdesign/global/dialog/touch/asset_design/content/items/fixedcol/items/col1/items/image"
                                                                        resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                                                    <imageAttributes
                                                                        jcr:primaryType="nt:unstructured"
                                                                        jcr:title="Asset Attributes"
                                                                        granite:class="js-cq-IPEPlugin-group"
                                                                        path="/apps/aemdesign/global/dialog/touch/asset/content/items/imageTab/items/column/items/assetAttributes"
                                                                        resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                                                </items>
                                                            </col1>
                                                        </items>
                                                    </fixedcol>
                                                </items>
                                            </imageTab>

                                            <layoutTab
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Layout"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                maximized="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <fixedcol
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                                        maximized="{Boolean}true">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <col1
                                                                jcr:primaryType="nt:unstructured"
                                                                granite:class="foundation-layout-util-maximized-alt"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <imageTab
                                                                        jcr:primaryType="nt:unstructured"
                                                                        jcr:title="Layout"
                                                                        granite:class="js-cq-IPEPlugin-group"
                                                                        path="/apps/aemdesign/components/media/image/v2/image/cq:dialog/content/items/layoutTab/items/column1/items/layoutConfig"
                                                                        resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                                                </items>
                                                            </col1>
                                                        </items>
                                                    </fixedcol>
                                                </items>
                                            </layoutTab>

                                            <cardConfigTab
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Card Config"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                maximized="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <fixedcol
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                                        maximized="{Boolean}true">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <col1
                                                                jcr:primaryType="nt:unstructured"
                                                                granite:class="foundation-layout-util-maximized-alt"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <imageTab
                                                                        jcr:primaryType="nt:unstructured"
                                                                        jcr:title="Card Config"
                                                                        granite:class="js-cq-IPEPlugin-group"
                                                                        path="/apps/aemdesign/components/media/image/v2/image/cq:dialog/content/items/layoutTab/items/column2/items/cardConfig/items/cardConfig"
                                                                        resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                                                </items>
                                                            </col1>
                                                        </items>
                                                    </fixedcol>
                                                </items>
                                            </cardConfigTab>

                                            <imageConfigTab
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Image Config"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                maximized="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <fixedcol
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                                        maximized="{Boolean}true">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <col1
                                                                jcr:primaryType="nt:unstructured"
                                                                granite:class="foundation-layout-util-maximized-alt"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <imageTab
                                                                        jcr:primaryType="nt:unstructured"
                                                                        jcr:title="Image Config"
                                                                        granite:class="js-cq-IPEPlugin-group"
                                                                        path="/apps/aemdesign/components/media/image/v2/image/cq:dialog/content/items/layoutTab/items/column2/items/imageConfig/items/imageConfig"
                                                                        resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                                                </items>
                                                            </col1>
                                                        </items>
                                                    </fixedcol>
                                                </items>
                                            </imageConfigTab>

                                            <responsiveConfigTab
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Responsive Config"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                maximized="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <fixedcol
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                                        maximized="{Boolean}true">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <col1
                                                                jcr:primaryType="nt:unstructured"
                                                                granite:class="foundation-layout-util-maximized-alt"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <imageTab
                                                                        jcr:primaryType="nt:unstructured"
                                                                        jcr:title="Responsive Config"
                                                                        granite:class="js-cq-IPEPlugin-group"
                                                                        path="/apps/aemdesign/components/media/image/v2/image/cq:dialog/content/items/layoutTab/items/column2/items/responsiveConfig/items/responsiveConfig"
                                                                        resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                                                </items>
                                                            </col1>
                                                        </items>
                                                    </fixedcol>
                                                </items>
                                            </responsiveConfigTab>

                                            <adaptiveConfigTab
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Adaptive Config"
                                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                                maximized="{Boolean}true">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <fixedcol
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                                        maximized="{Boolean}true">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <col1
                                                                jcr:primaryType="nt:unstructured"
                                                                granite:class="foundation-layout-util-maximized-alt"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <imageTab
                                                                        jcr:primaryType="nt:unstructured"
                                                                        jcr:title="Adaptive Config"
                                                                        granite:class="js-cq-IPEPlugin-group"
                                                                        path="/apps/aemdesign/components/media/image/v2/image/cq:dialog/content/items/layoutTab/items/column2/items/adaptiveConfig/items/adaptiveConfig"
                                                                        resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                                                </items>
                                                            </col1>
                                                        </items>
                                                    </fixedcol>
                                                </items>
                                            </adaptiveConfigTab>

                                            <style
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Style"
                                                path="/apps/aemdesign/global/dialog/touch/style_design/content"
                                                resourceType="granite/ui/components/coral/foundation/container"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                            <accessibility
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Accessibility"
                                                path="/apps/aemdesign/global/dialog/touch/accessibility_design/content"
                                                resourceType="granite/ui/components/coral/foundation/container"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                            <analytics
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Analytics"
                                                path="/apps/aemdesign/global/dialog/touch/analytics_design/content"
                                                resourceType="granite/ui/components/coral/foundation/container"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>

                                        </items>
                                    </accordion>
                                </items>
                            </content>
                        </items>
                    </other>
                </items>
            </tabs>

        </items>
    </content>
</jcr:root>
