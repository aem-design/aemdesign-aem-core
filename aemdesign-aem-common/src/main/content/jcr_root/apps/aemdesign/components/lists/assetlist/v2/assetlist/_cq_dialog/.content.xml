<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Asset List"
          helpPath="/mnt/overlay/wcm/core/content/sites/components/details.html/apps/aemdesign/components/lists/assetlist/v2/assetlist"
          sling:resourceType="cq/gui/components/authoring/dialog"
          extraClientlibs="[aem.design.components.coral.widgets.form.assetoptions]">
    <content
        granite:id="dialog-assetlist"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            type="nav"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">


            <assetTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Assets"
                sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <assets
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Assets"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <listFrom
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        class="cq-dialog-dropdown-showhide"
                                        cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                        fieldLabel="Build list using"
                                        emptyOption="{Boolean}true"
                                        name="./listFrom">
                                        <items jcr:primaryType="nt:unstructured">
                                            <children
                                                jcr:primaryType="nt:unstructured"
                                                text="Children"
                                                value="children"/>
                                            <descendants
                                                jcr:primaryType="nt:unstructured"
                                                text="Descendants"
                                                value="descendants"/>
                                            <static
                                                jcr:primaryType="nt:unstructured"
                                                text="Fixed list"
                                                value="static"/>
                                        </items>
                                    </listFrom>
                                    <setChildren
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="children">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for Child Assets"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <parentTag
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                        fieldDescription="Leave empty to use current page"
                                                        fieldLabel="Parent page"
                                                        name="./parentPath"
                                                        rootPath="/content/dam"/>
                                                </items>
                                            </well>
                                        </items>
                                    </setChildren>
                                    <setDescendants
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="descendants">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for Descendant Assets"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <ancestorTag
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                        fieldDescription="Leave empty to use current page"
                                                        fieldLabel="Descendants of"
                                                        name="./ancestorPath"
                                                        rootPath="/content/dam"/>
                                                </items>
                                            </well>
                                        </items>
                                    </setDescendants>
                                    <setStatic
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="static">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for Fixed list of Assets"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <assets
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                        class="foundation-layout-util-maximized-alt">
                                                        <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            name="./assets"
                                                            rootPath="/content/dam"/>
                                                    </assets>
                                                    <assetsHint
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/hidden"
                                                        name="./assets@TypeHint"
                                                        value="String[]"/>
                                                </items>
                                            </well>
                                        </items>
                                    </setStatic>


                                </items>
                            </assets>
                        </items>
                    </column1>
                    <column2
                        jcr:primaryType="nt:unstructured"
                        class="u-coral-padding u-coral-noMargin"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Config"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <orderBy
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="Enter a property like 'jcr:created'"
                                        fieldLabel="Order by"
                                        emptyOption="{Boolean}true"
                                        name="./orderBy">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            variant="valuelist"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/components/lists/taglist/orderby"/>
                                    </orderBy>
                                    <sortOrder
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="Sort order ascending or descending"
                                        fieldLabel="Sort Order"
                                        name="./sortOrder"
                                        type="editable">
                                        <items jcr:primaryType="nt:unstructured">
                                            <asc
                                                jcr:primaryType="nt:unstructured"
                                                text="Ascending"
                                                value="asc"/>
                                            <desc
                                                jcr:primaryType="nt:unstructured"
                                                text="Descending"
                                                value="desc"/>
                                        </items>
                                    </sortOrder>
                                    <limit
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldDescription="Maximum number of items displayed in list default is 100"
                                        fieldLabel="Limit"
                                        name="./limit"/>
                                </items>
                            </basic>
                        </items>
                    </column2>
                </items>
            </assetTab>

            <layoutTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Layout"
                sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Layout"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <variant
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldLabel="Variant"
                                        name="./variant"
                                        emptyOption="{Boolean}true"
                                        value="">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            variant="valuelist"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/components/lists/assetlist/variant"/>
                                    </variant>

                                    <titleType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        emptyOption="{Boolean}true"
                                        fieldDescription="which paragraph tag to use for Title display"
                                        fieldLabel="Title Type"
                                        name="./titleType"
                                        value="">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            variant="valuelist"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/html/heading"/>
                                    </titleType>

                                </items>
                            </variant>
                            <imageConfig
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Image"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <imageOption
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        emptyOption="{Boolean}true"
                                        fieldDescription="Image selection options for rendering image"
                                        fieldLabel="Image Option"
                                        name="./imageOption"
                                        class="dialog-dropdown-showhide"
                                        dialog-dropdown-showhide-target=".assetlist-imageoption-showhide-target"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                jcr:primaryType="nt:unstructured"
                                                text="Default"
                                                value=""/>
                                            <!--<generated-->
                                            <!--jcr:primaryType="nt:unstructured"-->
                                            <!--valuealt="generated"-->
                                            <!--text="Generate: generate new image with current image edits"-->
                                            <!--value="generated"/>-->
                                            <rendition
                                                jcr:primaryType="nt:unstructured"
                                                valuealt="generated"
                                                text="Rendition: pick a suitable image rendition"
                                                value="rendition"/>
                                            <responsive
                                                jcr:primaryType="nt:unstructured"
                                                text="Responsive: use existing renditions"
                                                value="responsive"/>
                                            <adaptive
                                                jcr:primaryType="nt:unstructured"
                                                text="Adaptive: generate new renditions with quality settings for each breakpoint"
                                                value="adaptive"/>
                                            <!--<mediaqueryrendition-->
                                            <!--jcr:primaryType="nt:unstructured"-->
                                            <!--text="Manual: responsive background with custom renditions per media query"-->
                                            <!--value="mediaqueryrendition"/>-->
                                        </items>
                                    </imageOption>
                                </items>
                            </imageConfig>
                        </items>
                    </column1>
                    <column2
                        jcr:primaryType="nt:unstructured"
                        class="u-coral-padding u-coral-noMargin"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">

                            <imageConfig
                                jcr:primaryType="nt:unstructured"
                                class="hide assetlist-imageoption-showhide-target foundation-layout-util-vmargin"
                                showhidetargetvalue="generated"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <imageConfig
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Image Config"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">

                                            <imageDimensionHeading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                fieldDescription="This will be used to pick suitable size of rendition."
                                                text="Image Dimension"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">

                                                    <imageDimension
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <imageSize
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/sizefield"
                                                                fieldDescription=""
                                                                fieldLabel="Image Size (w,h)">
                                                                <width
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                                    name="./width"/>
                                                                <height
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                                    name="./height"/>
                                                            </imageSize>
                                                        </items>
                                                    </imageDimension>

                                                    <resizingProportionsHeading
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/heading"
                                                        class="coral-Heading coral-Heading--4"
                                                        level="{Long}4"
                                                        granite:hidden="{Boolean}true"
                                                        text="Resizing Proportions"/>
                                                    <resizingProportions
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <min
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/sizefield"
                                                                fieldLabel="Minimal Size (w,h)">
                                                                <width
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                                    name="./minWidth"/>
                                                                <height
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                                    name="./minHeight"/>
                                                            </min>
                                                            <max
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/sizefield"
                                                                fieldLabel="Maximal Size (w,h)">
                                                                <width
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                                    name="./maxWidth"/>
                                                                <height
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                                    name="./maxHeight"/>
                                                            </max>
                                                        </items>
                                                    </resizingProportions>

                                                </items>
                                            </well>
                                        </items>
                                    </imageConfig>

                                </items>
                            </imageConfig>
                            <responsiveConfig
                                jcr:primaryType="nt:unstructured"
                                class="hide assetlist-imageoption-showhide-target foundation-layout-util-vmargin"
                                showhidetargetvalue="responsive"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <responsiveConfig
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Responsive Config"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">

                                            <responsiveRenditionSizeHeading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Responsive Rendition"/>

                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">


                                                    <responsiveRenditionSize
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <responsiveImageMapping
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                fieldDescription="setup map of rendition size to viewport"
                                                                fieldLabel="Rendition Image Mapping">
                                                                <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    name="./renditionImageMapping"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                                                            </responsiveImageMapping>
                                                            <note1
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/note"
                                                                note="format: [rendition width]=[media query]"/>
                                                            <note2
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/note"
                                                                note="- ex 48=(min-width: 1px) and (max-width: 100px), this will show rendition of 48px between 1px and 100px device width"/>
                                                            <note3
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/note"
                                                                note="- ex 1280=(min-width: 478px) will use rendition 1280 starting from width of 487 and up"/>
                                                        </items>
                                                    </responsiveRenditionSize>

                                                    <renditionPrefix
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                                        emptyOption="{Boolean}true"
                                                        fieldDescription="Rendition prefix to use as priority for selecting renditions, otherwise best match will be returned"
                                                        fieldLabel="Rendition Prefix"
                                                        name="./renditionPrefix"
                                                        value="">
                                                        <datasource
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                            variant="valuelist"
                                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/asset/rendition/prefix"/>
                                                    </renditionPrefix>
                                                </items>
                                            </well>

                                        </items>
                                    </responsiveConfig>
                                </items>
                            </responsiveConfig>

                            <adaptiveConfig
                                jcr:primaryType="nt:unstructured"
                                class="hide assetlist-imageoption-showhide-target foundation-layout-util-vmargin"
                                showhidetargetvalue="adaptive"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <adaptiveConfig
                                        jcr:primaryType="nt:unstructured"
                                        jcr:title="Adaptive Config"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">

                                            <adaptiveRenditionSize
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Responsive Rendition"/>

                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <adaptiveRenditionSize
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <adaptiveImageMapping
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                fieldDescription="setup map of quality setting to view port"
                                                                fieldLabel="Adaptive Image Mapping">
                                                                <field
                                                                    jcr:primaryType="nt:unstructured"
                                                                    name="./adaptiveImageMapping"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
                                                            </adaptiveImageMapping>
                                                            <note1
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/note"
                                                                note="format: [rendition width]=[media query]"/>
                                                            <note2
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/note"
                                                                note="- ex 48=(min-width: 1px) and (max-width: 100px), this will show rendition of 48px between 1px and 100px device width"/>
                                                            <note3
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="cq/gui/components/authoring/dialog/note"
                                                                note="- ex 1280=(min-width: 478px) will use rendition 1280 starting from width of 487 and up"/>
                                                        </items>
                                                    </adaptiveRenditionSize>
                                                </items>
                                            </well>


                                        </items>
                                    </adaptiveConfig>
                                </items>
                            </adaptiveConfig>


                        </items>
                    </column2>
                </items>
            </layoutTab>


            <style
                jcr:primaryType="nt:unstructured"
                jcr:title="Style"
                path="/apps/aemdesign/global/dialog/touch/style/content/items/tabs/items/styleTab"
                resourceType="granite/ui/components/foundation/section"
                sling:resourceType="granite/ui/components/coral/foundation/include"/>
            <accessibility
                jcr:primaryType="nt:unstructured"
                jcr:title="Accessibility"
                path="/apps/aemdesign/global/dialog/touch/accessibility/content/items/tabs/items/accessibilityTab"
                resourceType="granite/ui/components/foundation/section"
