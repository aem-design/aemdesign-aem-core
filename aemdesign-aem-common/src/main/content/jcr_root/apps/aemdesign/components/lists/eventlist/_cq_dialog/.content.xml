<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Event List"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="en/cq/current/wcm/default_components.html#List">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <eventListTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Event List"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <listFrom
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Build list using"
                                name="./listFrom"
                                value="">
                                <items jcr:primaryType="nt:unstructured">
                                    <children
                                        jcr:primaryType="nt:unstructured"
                                        text="Child pages"
                                        value="children"/>
                                    <descendants
                                        jcr:primaryType="nt:unstructured"
                                        text="Descendant pages"
                                        value="descendants"/>
                                    <static
                                        jcr:primaryType="nt:unstructured"
                                        text="Fixed list"
                                        value="static"/>
                                    <tags
                                        jcr:primaryType="nt:unstructured"
                                        text="Tags"
                                        value="tags"/>
                                    <search
                                        jcr:primaryType="nt:unstructured"
                                        text="Search"
                                        value="search"/>
                                    <querybuilder
                                        jcr:primaryType="nt:unstructured"
                                        text="Advanced Search"
                                        value="querybuilder"/>
                                </items>
                            </listFrom>
                            <orderBy
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Enter a property like 'jcr:created'"
                                fieldLabel="Order by"
                                name="./orderBy">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        value="jcr:title"/>
                                    <created
                                        jcr:primaryType="nt:unstructured"
                                        value="jcr:created"/>
                                    <modified
                                        jcr:primaryType="nt:unstructured"
                                        value="cq:lastModified"/>
                                    <template
                                        jcr:primaryType="nt:unstructured"
                                        value="cq:template"/>
                                </items>
                            </orderBy>
                            <limit
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                fieldDescription="Maximum number of items displayed in list"
                                fieldLabel="Limit"
                                name="./limit"/>
                            <detailsBadge
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Which badge to use from Details component"
                                fieldLabel="Badges"
                                name="./detailsBadge"
                                value="">
                                <items jcr:primaryType="nt:unstructured">
                                    <default
                                        jcr:primaryType="nt:unstructured"
                                        text="Default"
                                        value=""/>
                                    <imageTitleDateLocationDescriptionReference
                                        jcr:primaryType="nt:unstructured"
                                        text="Image Title Date Location Description Reference"
                                        value="imageTitleDateLocationDescriptionReference"/>
                                    <imageTitleDate
                                        jcr:primaryType="nt:unstructured"
                                        text="Image Title Date"
                                        value="imageTitleDate"/>
                                    <imageTitleDescriptionCategoryMansory
                                        jcr:primaryType="nt:unstructured"
                                        text="Image Title Description Category (Mansory)"
                                        value="imageTitleDescriptionCategoryMansory"/>
                                    <imageTitleDescriptionCategory
                                        jcr:primaryType="nt:unstructured"
                                        text="Image Title Description Category"
                                        value="imageTitleDescriptionCategory"/>
                                    <titleDateTimeCategory
                                        jcr:primaryType="nt:unstructured"
                                        text="Title Date Time Category"
                                        value="titleDateTimeCategory"/>
                                    <dateTimeLocationEnquiryInfo
                                        jcr:primaryType="nt:unstructured"
                                        text="Date Time Location Enquiry Info"
                                        value="dateTimeLocationEnquiryInfo"/>
                                    <aPartOf
                                        jcr:primaryType="nt:unstructured"
                                        text="A Part Of"
                                        value="aPartOf"/>
                                    <imageTitleDateLocationDescriptionReference2
                                        jcr:primaryType="nt:unstructured"
                                        text="Image Title Date Location Description Multi References"
                                        value="imageTitleDateLocationDescriptionReference2"/>
                                </items>
                            </detailsBadge>
                        </items>
                    </column>
                </items>
            </eventListTab>
            <childrenTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Child pages"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <parentPage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="Leave empty to use current page"
                                fieldLabel="Parent page"
                                name="./parentPage"/>
                        </items>
                    </column>
                </items>
            </childrenTab>
            <descendantsTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Descendant pages"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <ancestorPage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="Leave empty to use current page"
                                fieldLabel="Parent page"
                                name="./ancestorPage"/>
                        </items>
                    </column>
                </items>
            </descendantsTab>
            <staticTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Fixed list"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <pages
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                fieldDescription="Click the '+' to add a new page"
                                fieldLabel="Pages">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    name="./pages"
                                    required="{Boolean}false"/>
                            </pages>
                        </items>
                    </column>
                </items>
            </staticTab>
            <tagsTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Tags"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <parentPage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="Leave empty to use current page"
                                fieldLabel="Parent page"
                                name="./tagsSearchRoot"/>
                            <tags
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Tags/Keywords"
                                multiple="{Boolean}true"
                                name="./tags"/>
                            <match
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Match"
                                name="./tagsMatch"
                                value="any">
                                <items jcr:primaryType="nt:unstructured">
                                    <any
                                        jcr:primaryType="nt:unstructured"
                                        text="any tag"
                                        value="any"/>
                                    <all
                                        jcr:primaryType="nt:unstructured"
                                        text="all tags"
                                        value="all"/>
                                </items>
                            </match>
                        </items>
                    </column>
                </items>
            </tagsTab>
            <searchTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Search"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <searchIn
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="Leave empty to use current site (eg. /content/mysite)"
                                fieldLabel="Start in"
                                name="./searchIn"/>
                            <query
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Search query"
                                name="./query"/>
                        </items>
                    </column>
                </items>
            </searchTab>
            <querybuilderTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Advanced Search"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <savedquery
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                fieldLabel="Querybuilder predicate notation"
                                name="./savedquery"/>
                        </items>
                    </column>
                </items>
            </querybuilderTab>
            <layoutTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Layout"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Variant"
                                name="./variant"
                                value="">
                                <items jcr:primaryType="nt:unstructured">
                                    <default
                                        jcr:primaryType="nt:unstructured"
                                        text="Default"
                                        value=""/>
                                    <masonry
                                        jcr:primaryType="nt:unstructured"
                                        text="Masonry"
                                        value="masonry"/>
                                </items>
                            </variant>
                            <printStructure
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Don't print structure of the list, omit (OL/UL and LI tags)"
                                fieldLabel="Print List Structure"
                                name="./printStructure"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </printStructure>
                            <listcontainer
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="CSS for List Container"
                                name="./cssClassListContainer"/>
                            <list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="CSS for List"
                                name="./cssClassList"/>
                            <item
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="CSS for Item"
                                name="./cssClassItem"/>
                            <itemlink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="CSS for Item Link"
                                name="./cssClassItemLink"/>
                        </items>
                    </column>
                </items>
            </layoutTab>
            <advanceTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Advanced"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <topic
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Subscribers waiting for notification of the filter value is updated"
                                fieldLabel="Topic Queue"
                                name="./topicQueue"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </topic>
                        </items>
                    </column>
                </items>
            </advanceTab>
            <styleTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Style"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <componentId
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="ID"
                                name="./componentId"
                                required="{Boolean}false"/>
                            <componentTheme
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Theme"
                                multiple="{Boolean}true"
                                name="./componentTheme"/>
                            <componentModifiers
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Modifiers"
                                multiple="{Boolean}true"
                                name="./componentModifiers"/>
                            <componentModule
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Module"
                                multiple="{Boolean}true"
                                name="./componentModule"/>
                            <componentChevron
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Chevron"
                                multiple="{Boolean}true"
                                name="./componentChevron"/>
                            <componentIcon
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Icon"
                                multiple="{Boolean}true"
                                name="./componentIcon"/>
                            <componentBooleanAttrs
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Boolean Attributes"
                                multiple="{Boolean}true"
                                name="./componentBooleanAttrs"/>
                            <position
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Position coordinates"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <positions
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/sizefield"
                                        fieldDescription="The X and Y values of the Position"
                                        fieldLabel="Positions">
                                        <width
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                            name="./positionX"
                                            unit="X position"/>
                                        <height
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                            name="./positionY"
                                            unit="Y position"/>
                                    </positions>
                                </items>
                            </position>
                            <animation
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Animation"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <animationConfigGroupLabel/>
                                    <animationEnabled
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="Should the animation be enabled for this component?"
                                        fieldLabel="Enable animations"
                                        name="./animationEnabled"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationEnabled>
                                    <animationName
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="The type of animation to run"
                                        fieldLabel="Name"
                                        name="./animationName"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationName>
                                    <animationOnce
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="Should the animation only run once? By default is runs everytime you scroll past the element on the page"
                                        fieldLabel="Run Once?"
                                        name="./animationOnce"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationOnce>
                                    <animationEasing
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="How should the animation play?"
                                        fieldLabel="Easing"
                                        name="./animationEasing"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationEasing>
                                    <animationDelay
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        defaultValue=""
                                        fieldDescription="How many milliseconds should occur before the animation runs?"
                                        fieldLabel="Delay"
                                        name="./animationDelay"
                                        required="{Boolean}false"
                                        step="100"/>
                                    <animationDuration
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        defaultValue=""
                                        fieldDescription="How long should the animation run for? Minimum is '400' and the maximum is '3000'. Values must be in increments of 50, i.e. 450, 500, 550..."
                                        fieldLabel="Duration"
                                        name="./animationDuration"
                                        required="{Boolean}false"
                                        step="50"/>
                                </items>
                            </animation>
                        </items>
                    </column>
                </items>
            </styleTab>
            <accessibilityTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Accessibility"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <ariaLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Defines a string value that labels the current element."
                                fieldLabel="Aria Label"
                                name="./ariaLabel"
                                required="{Boolean}false"/>
                            <ariaRole
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Main indicator of type. This semantic association allows tools to present and support interaction with the object in a manner that is consistent with user expectations about other objects of that type."
                                fieldLabel="Aria Role"
                                name="./ariaRole"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaRole>
                            <ariaDescribedBy
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) that describes the object."
                                fieldLabel="Aria Described By"
                                name="./ariaDescribedBy"
                                required="{Boolean}false"/>
                            <ariaLabelledBy
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) that labels the current element."
                                fieldLabel="Aria Labelled By"
                                name="./ariaLabelledBy"
                                required="{Boolean}false"/>
                            <ariaControls
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) whose contents or presence are controlled by the current element. "
                                fieldLabel="Aria Controls"
                                name="./ariaControls"
                                required="{Boolean}false"/>
                            <ariaLive
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Indicates that an element will be updated, and describes the types of updates the user agents, assistive technologies, and user can expect from the live region."
                                fieldLabel="Aria Live"
                                name="./ariaLive"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaLive>
                            <ariaHidden
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Indicates that the element and all of its descendants are not visible or perceivable to any user as implemented by the author."
                                fieldLabel="Aria Hidden"
                                name="./ariaHidden"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaHidden>
                            <ariaHaspopup
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Indicates that the element has a popup context menu or sub-level menu."
                                fieldLabel="Aria Has-Popup"
                                name="./ariaHaspopup"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaHaspopup>
                            <ariaAccessKey
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) whose contents or presence are controlled by the current element. "
                                fieldLabel="Access Key"
                                name="./ariaAccessKey"
                                required="{Boolean}false"/>
                        </items>
                    </column>
                </items>
            </accessibilityTab>
        </items>
    </content>
</jcr:root>
