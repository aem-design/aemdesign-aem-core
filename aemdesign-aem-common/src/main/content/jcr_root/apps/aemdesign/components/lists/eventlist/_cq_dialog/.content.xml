                                                    text="Icon"
                                                    value="icon"/>
                                            <image
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Image"
                                                    value="image"/>
                                            <card
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Image"
                                                    value="card"/>
                                            <cardIcon
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Icon"
                                                    value="cardIcon"/>
                                            <cardIconAction
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Icon and CTA"
                                                    value="cardIconAction"/>
                                            <cardAction
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Image and CTA"
                                                    value="cardAction"/>
                                            <cardIconSelect
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Clickable Card with Icon"
                                                    value="cardIconSelect"/>
