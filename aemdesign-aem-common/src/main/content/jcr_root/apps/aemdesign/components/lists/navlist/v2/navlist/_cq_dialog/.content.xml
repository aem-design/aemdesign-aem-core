<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Nav List"
          helpPath="/mnt/overlay/wcm/core/content/sites/components/details.html/apps/aemdesign/components/lists/navlist/v2/navlist"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            type="nav"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">

            <inheritTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Inherit"
                path="/apps/aemdesign/global/dialog/touch/inherit/content/items/inheritTab"
                resourceType="granite/ui/components/foundation/section"
                sling:resourceType="granite/ui/components/coral/foundation/include"/>

            <listTab
                jcr:primaryType="nt:unstructured"
                jcr:title="List"
                sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                jcr:primaryType="nt:unstructured"
                                jcr:title="List"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <depthFromRoot
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldDescription="will only collect defined number of items max of 5"
                                        fieldLabel="Levels of children to collect"
                                        min="0"
                                        max="5"
                                        name="./depthFromRoot"/>
                                    <listFrom
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        class="cq-dialog-dropdown-showhide"
                                        cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                        fieldLabel="Build list using"
                                        emptyOption="{Boolean}true"
                                        name="./listFrom">
                                        <items jcr:primaryType="nt:unstructured">
                                            <children
                                                jcr:primaryType="nt:unstructured"
                                                text="Child pages"
                                                value="children"/>
                                            <static
                                                jcr:primaryType="nt:unstructured"
                                                text="Fixed list"
                                                value="static"/>
                                        </items>
                                    </listFrom>
                                    <setChildren
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="children">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for Child Pages"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <parentPage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                        fieldDescription="Leave empty to use current page"
                                                        fieldLabel="Parent page"
                                                        name="./parentPage"
                                                        rootPath="/content"/>
                                                </items>
                                            </well>
                                        </items>
                                    </setChildren>
                                    <setStatic
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="static">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for Fixed list"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pages
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                        class="foundation-layout-util-maximized-alt">
                                                        <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            name="./pages"
                                                            rootPath="/content"/>
                                                    </pages>
                                                </items>
                                            </well>
                                        </items>
                                    </setStatic>
                                </items>
                            </basic>
                        </items>
                    </column1>
                </items>
            </listTab>


            <layoutTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Layout"
                sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Layout"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <linkTitlePrefix
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Text prefix for Nav link title '{Prefix} {Link Title}'"
                                        fieldLabel="Title Prefix"
                                        name="./linkTitlePrefix"/>

                                    <variant
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldLabel="Variant"
                                        class="cq-dialog-dropdown-showhide"
                                        cq-dialog-dropdown-showhide-target=".list-option-variant-showhide-target"
                                        name="./variant"
                                        emptyOption="{Boolean}true"
                                        value="">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            variant="valuelist"
                                            path="/content/cq:tags/${namespace}/component-dialog/components/lists/navlist/variant"/>
                                    </variant>
                                    <variantSimple
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-variant-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="simple">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Menu Title"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <menuTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                        fieldLabel="Title"
                                                        name="./menuTitle"/>
                                                </items>
                                            </well>
                                        </items>
                                    </variantSimple>


                                </items>
                            </variant>
                        </items>
                    </column>
                </items>
            </layoutTab>


            <style
                jcr:primaryType="nt:unstructured"
                jcr:title="Style"
                path="/apps/aemdesign/global/dialog/touch/style/content/items/tabs/items/styleTab"
                resourceType="granite/ui/components/foundation/section"
                sling:resourceType="granite/ui/components/coral/foundation/include"/>
            <accessibility
                jcr:primaryType="nt:unstructured"
                jcr:title="Accessibility"
                path="/apps/aemdesign/global/dialog/touch/accessibility/content/items/tabs/items/accessibilityTab"
                resourceType="granite/ui/components/foundation/section"
                sling:resourceType="granite/ui/components/coral/foundation/include"/>
            <analytics
                jcr:primaryType="nt:unstructured"
                jcr:title="Analytics"
                path="/apps/aemdesign/global/dialog/touch/analytics/content/items/tabs/items/analyticsTab"
                resourceType="granite/ui/components/foundation/section"
                sling:resourceType="granite/ui/components/coral/foundation/include"/>
        </items>
    </content>

</jcr:root>

