<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Event details"
    xtype="dialog"
    height="400"
    width="600">

    <items jcr:primaryType="cq:WidgetCollection">
        <tabs jcr:primaryType="cq:TabPanel">
            <items jcr:primaryType="cq:WidgetCollection">

                <eventdetailstab jcr:primaryType="cq:Panel"
                         title="Event Details">

                    <items jcr:primaryType="cq:WidgetCollection">
                        <title
                            jcr:primaryType="cq:Widget"
                            path="/apps/aemdesign/global/dialog/classic/page/items/pageBasicTab/items/title.infinity.json"
                            xtype="cqinclude"/>

                        <descriptionLabel
                                jcr:primaryType="cq:Widget"
                                text="Description"
                                xtype="label"/>
                        <description jcr:primaryType="cq:Widget"
                            fieldLabel="Description"
                            fieldDescription="Leave empty to use current page description"
                            hideLabel="{Boolean}true"
                            name="./description"
                            xtype="richtext">
                            <rtePlugins jcr:primaryType="nt:unstructured"
                                                path="/apps/aemdesign/global/dialog/classic/rteprofile/items/advanced/items/text/rtePlugins.infinity.json"
                                                xtype="cqinclude" />
                        </description>
                        <eventStartDate
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Event Start Date"
                                fieldDescription=""
                                allowBlank="{Boolean}false"
                                defaultValue=""
                                format="d.m.Y H:m"
                                name="./eventStartDate"
                                xtype="datetime"/>
                        <eventEndDate
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Event End Date"
                                fieldDescription=""
                                allowBlank="{Boolean}false"
                                defaultValue=""
                                format="d.m.Y H:m"
                                name="./eventEndDate"
                                xtype="datetime"/>
                        <eventLocation
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Event Location"
                                fieldDescription=""
                                allowBlank="{Boolean}true"
                                name="./eventLoc"
                                xtype="textfield"/>
                        <eventRefLabel
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Event Reference Label"
                                fieldDescription=""
                                allowBlank="{Boolean}true"
                                name="./eventRefLabel"
                                xtype="textfield"/>
                        <eventRefLink
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Event Reference Link"
                                fieldDescription=""
                                allowBlank="{Boolean}true"
                                name="./eventRefLink"
                                xtype="pathfield"/>
                        <eventRefLabel2
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Other Event Reference Label"
                                fieldDescription=""
                                allowBlank="{Boolean}true"
                                name="./eventRefLabel2"
                                xtype="textfield"/>
                        <eventRefLink2
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Other Event Reference Link"
                                fieldDescription=""
                                allowBlank="{Boolean}true"
                                name="./eventRefLink2"
                                xtype="pathfield"/>
                        <enquiryInfo
                                jcr:primaryType="cq:Widget"
                                fieldLabel="Enquiry Information"
                                fieldDescription=""
                                allowBlank="{Boolean}true"
                                name="./enquiryInfo"
                                xtype="textarea"/>
                        <category
                                jcr:primaryType="cq:Widget"
                                path="/apps/aemdesign/global/dialog/classic/page/items/pageBasicTab/items/category.infinity.json"
                                xtype="cqinclude"/>
                        <subCategory
                                jcr:primaryType="cq:Widget"
                                path="/apps/aemdesign/global/dialog/classic/page/items/pageBasicTab/items/subCategory.infinity.json"
                                xtype="cqinclude"/>

                        <mixinTypes
                                jcr:primaryType="cq:Widget"
                                path="/apps/aemdesign/global/dialog/classic/page/items/pageBasicTab/items/mixinTypes.infinity.json"
                                xtype="cqinclude"/>
                        <mixinTypesHint
                                jcr:primaryType="cq:Widget"
                                path="/apps/aemdesign/global/dialog/classic/page/items/pageBasicTab/items/mixinTypesHint.infinity.json"
                                xtype="cqinclude"/>

                    </items>

                </eventdetailstab>

                <imageBackgroundTab
                        jcr:primaryType="cq:Widget"
                        path="/apps/aemdesign/global/dialog/classic/images/items/imageBackgroundTab.infinity.json"
                        xtype="cqinclude"/>

                <imageBackgroundTabResourceType
                        jcr:primaryType="cq:Widget"
                        path="/apps/aemdesign/global/dialog/classic/images/items/imageBackgroundTabResourceType.infinity.json"
                        xtype="cqinclude"/>


                <layoutTab jcr:primaryType="cq:Panel"
                            title="Layout">

                    <items jcr:primaryType="cq:WidgetCollection">
                        <variant
                                jcr:primaryType="cq:Widget"
                                defaultValue=""
                                value=""
                                allowBlank="{Boolean}true"
                                fieldLabel="Variant"
                                name="./variant"
                                type="select"
                                xtype="selection">
                            <options jcr:primaryType="cq:WidgetCollection">
                                <default
                                        jcr:primaryType="nt:unstructured"
                                        value=""
                                        text="default"/>
                                <hidden
                                        jcr:primaryType="nt:unstructured"
                                        value="hidden"
                                        text="Hidden"/>
                            </options>
                        </variant>
                        <promotionSize jcr:primaryType="cq:Widget"
                                       fieldLabel="Block Size"
                                       fieldDescription="this will be used as card size in grids"
                                       allowBlank="{Boolean}true"
                                       name="./cardSize"
                                       options="/etc/tags/component-style/theme/common/card/size.valuelist.json"
                                       type="select"
                                       defaultValue=""
                                       value=""
                                       xtype="selection"/>

                        <titleFormats
                                jcr:primaryType="cq:Widget"
                                collapsed="{Boolean}false"
                                collapsible="{Boolean}true"
                                title="Title Formats"
                                xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <titleFormat jcr:primaryType="cq:Widget"
                                             fieldLabel="Title Format"
                                             fieldDescription="Format of Title to be used for display"
                                             allowBlank="{Boolean}true"
                                             name="./titleFormat"
                                             options="/etc/tags/component-style/theme/details/event-details/format/title.valuelist.pathvalue.json"
                                             type="select"
                                             defaultValue=""
                                             xtype="selection"/>
                                <subTitleFormat jcr:primaryType="cq:Widget"
                                                fieldLabel="Sub-Title Format"
                                                fieldDescription="Format of Sub Title to be used for display"
                                                allowBlank="{Boolean}true"
                                                name="./subTitleFormat"
                                                options="/etc/tags/component-style/theme/details/event-details/format/subtitle.valuelist.pathvalue.json"
                                                type="select"
                                                defaultValue=""
                                                xtype="selection"/>
                                <eventDisplayDateFormat jcr:primaryType="cq:Widget"
                                                fieldLabel="Event Display Date"
                                                fieldDescription="Format of Event Date to be used for display"
                                                allowBlank="{Boolean}true"
                                                name="./eventDisplayDateFormat"
                                                options="/etc/tags/component-style/theme/details/event-details/format/displaydate.valuelist.pathvalue.json"
                                                type="select"
                                                defaultValue=""
                                                xtype="selection"/>
                                <showTags jcr:primaryType="cq:Widget"
                                                        fieldLabel="Show Tags on Page"
                                                        fieldDescription="Show Tags on Page"
                                                        allowBlank="{Boolean}true"
                                                        name="./showTags"
                                                        options="/etc/tags/component-dialog/common/yes-no.valuelist.json"
                                                        type="select"
                                                        defaultValue=""
                                                        xtype="selection"/>
                            </items>
                        </titleFormats>


                    </items>

                </layoutTab>

                <badgeConfigTab jcr:primaryType="cq:Panel"
                             path="/apps/aemdesign/global/dialog/classic/badge/items/badgeConfigTab.infinity.json"
                             xtype="cqinclude" />

                <styleTab
                        jcr:primaryType="cq:Panel"
                        path="/apps/aemdesign/global/dialog/classic/style/items/styleTab.infinity.json"
                        xtype="cqinclude"/>
                <accessibilityTab jcr:primaryType="cq:Panel"
                                  path="/apps/aemdesign/global/dialog/classic/accessibility/items/accessibilityTab.infinity.json"
                                  xtype="cqinclude" />
            </items>
        </tabs>
    </items>


</jcr:root>
