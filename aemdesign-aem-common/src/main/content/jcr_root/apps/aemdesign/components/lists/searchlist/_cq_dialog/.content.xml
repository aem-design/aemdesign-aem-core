<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Search"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
        <layout
                jcr:primaryType="nt:unstructured"
                type="nav"
                sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">

            <basicTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Basic"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Basic"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <searchIn
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                            fieldLabel="Path to search in"
                                            name="./searchIn"/>
                                    <searchButtonText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Search button text"
                                            name="./searchButtonText"/>
                                    <statisticsText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Statistics text"
                                            name="./statisticsText"/>
                                    <searchQueryInformation
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Search query information"
                                            fieldDescriotion="Added to Statistics text field"
                                            name="./searchQueryInformation"/>
                                    <noResultsText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                            fieldLabel="No results text"
                                            name="./noResultsText"/>
                                    <spellcheckText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Spellcheck Text"
                                            name="./spellcheckText"/>
                                    <similarPagesText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Similar pages text"
                                            name="./similarPagesText"/>
                                    <relatedSearchesText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Related searches text"
                                            name="./relatedSearchesText"/>
                                    <searchTrendsText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Search trends text"
                                            name="./searchTrendsText"/>
                                    <resultPagesText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Result pages label"
                                            name="./resultPagesText"/>
                                    <previousText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Previous label"
                                            name="./previousText"/>
                                    <nextText
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Next label"
                                            name="./nextText"/>

                                </items>
                            </basic>
                        </items>
                    </column>
                </items>
            </basicTab>



            <layoutTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Layout"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Layout"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <variant
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldLabel="Variant"
                                            name="./variant"
                                            emptyOption="{Boolean}true"
                                            class="cq-dialog-dropdown-showhide"
                                            cq-dialog-dropdown-showhide-target=".variant-config-showhide-target"
                                            value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Default"
                                                    value=""/>
                                            <cards
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Cards"
                                                    value="cards"/>
                                        </items>
                                    </variant>
                                </items>
                            </variant>


                            <variantConfigCard
                                    jcr:primaryType="nt:unstructured"
                                    class="hide variant-config-showhide-target foundation-layout-util-vmargin"
                                    showhidetargetvalue="card"
                                    sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <childsection
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Card Config"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">

                                            <assetActionText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Asset Action Text"
                                                    name="./assetActionText"/>
                                            <pageActionText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Page Action Text"
                                                    name="./pageActionText"/>
                                            <otherActionText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Page Action Text"
                                                    name="./otherActionText"/>

                                        </items>
                                    </childsection>

                                </items>
                            </variantConfigCard>
                        </items>
                    </column>
                </items>
            </layoutTab>


            <styleTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Style"
                    path="/apps/aemdesign/global/dialog/touch/style/content/items/styleTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
            <accessibilityTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Accessibility"
                    path="/apps/aemdesign/global/dialog/touch/accessibility/content/items/accessibilityTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
        </items>
    </content>

</jcr:root>
