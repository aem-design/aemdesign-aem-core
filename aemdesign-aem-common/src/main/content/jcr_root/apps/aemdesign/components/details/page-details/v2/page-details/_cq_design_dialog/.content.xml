<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Page Details (Design)"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <plugins
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/accordion">
                <items jcr:primaryType="nt:unstructured">
                    <pageDetails
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Page Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                        jcr:primaryType="nt:unstructured"
                                        granite:class="foundation-layout-util-maximized-alt"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <pageDetails
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Page Details"
                                                granite:class="js-cq-IPEPlugin-group"
                                                path="/apps/aemdesign/global/dialog/touch/page/content/items/pageBasicTab/items/column"
                                                resourceType="granite/ui/components/coral/foundation/container"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </pageDetails>

                    <layout
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Layout"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                        jcr:primaryType="nt:unstructured"
                                        granite:class="foundation-layout-util-maximized-alt"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <layout
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Layout"
                                                granite:class="js-cq-IPEPlugin-group"
                                                path="/apps/aemdesign/components/details/page-details/v2/page-details/cq:dialog/content/items/layoutTab"
                                                resourceType="granite/ui/components/coral/foundation/container"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </layout>

                    <badgeConfigTab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Badge Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                        jcr:primaryType="nt:unstructured"
                                        granite:class="foundation-layout-util-maximized-alt"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <titleConfigGroup
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Title Configuration"
                                                path="/apps/aemdesign/global/dialog/touch/badge/content/items/tabs/items/badgeConfigTab/items/column/items/config/items/titleConfigGroup"
                                                resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                            <cardConfigGroup
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Card Badge Config"
                                                path="/apps/aemdesign/global/dialog/touch/badge/content/items/tabs/items/badgeConfigTab/items/column/items/config/items/cardConfigGroup"
                                                resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                            <badgeConfigGroup
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="General Badge Config"
                                                path="/apps/aemdesign/global/dialog/touch/badge/content/items/tabs/items/badgeConfigTab/items/column/items/config/items/badgeConfigGroup"
                                                resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                            <linkConfigGroup
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Badge CTA Config"
                                                path="/apps/aemdesign/global/dialog/touch/badge/content/items/tabs/items/badgeConfigTab/items/column/items/config/items/linkConfigGroup"
                                                resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                            <iconConfigGroup
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Icon Badge Config"
                                                path="/apps/aemdesign/global/dialog/touch/badge/content/items/tabs/items/badgeConfigTab/items/column/items/config/items/iconConfigGroup"
                                                resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                            <thumbnailConfig
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Thumbnail Config"
                                                path="/apps/aemdesign/global/dialog/touch/badge/content/items/tabs/items/badgeConfigTab/items/column/items/config/items/thumbnailConfig"
                                                resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                            <menuConfigGroup
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Menu Configuration"
                                                path="/apps/aemdesign/global/dialog/touch/badge/content/items/tabs/items/badgeConfigTab/items/column/items/config/items/menuConfigGroup"
                                                resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                            <tabConfigGroup
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Tab Configuration"
                                                path="/apps/aemdesign/global/dialog/touch/badge/content/items/tabs/items/badgeConfigTab/items/column/items/config/items/tabConfigGroup"
                                                resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                            <analyticsConfigGroup
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Analytics Configuration"
                                                path="/apps/aemdesign/global/dialog/touch/badge/content/items/tabs/items/badgeConfigTab/items/column/items/config/items/analyticsConfigGroup"
                                                resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </badgeConfigTab>

                    <secondaryImageTab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Secondary Image"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                        jcr:primaryType="nt:unstructured"
                                        granite:class="foundation-layout-util-maximized-alt"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <secondaryImageTab
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Secondary Image"
                                                granite:class="js-cq-IPEPlugin-group"
                                                path="/apps/aemdesign/global/dialog/touch/asset_design/content/items/fixedcol/items/col1/items/secondaryImageTab"
                                                resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </secondaryImageTab>

                    <imageBackgroundTab
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Background Image"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <fixedcol
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                maximized="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <col1
                                        jcr:primaryType="nt:unstructured"
                                        granite:class="foundation-layout-util-maximized-alt"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <imageBackgroundTab
                                                jcr:primaryType="nt:unstructured"
                                                jcr:title="Background Image"
                                                granite:class="js-cq-IPEPlugin-group"
                                                path="/apps/aemdesign/global/dialog/touch/asset_design/content/items/fixedcol/items/col1/items/imageBackgroundTab"
                                                resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                                sling:resourceType="granite/ui/components/coral/foundation/include"/>
                                        </items>
                                    </col1>
                                </items>
                            </fixedcol>
                        </items>
                    </imageBackgroundTab>

                    <style
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Style"
                        path="/apps/aemdesign/global/dialog/touch/style_design/content"
                        resourceType="granite/ui/components/coral/foundation/container"
                        sling:resourceType="granite/ui/components/coral/foundation/include"/>
                    <accessibility
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Accessibility"
                        path="/apps/aemdesign/global/dialog/touch/accessibility_design/content"
                        resourceType="granite/ui/components/coral/foundation/container"
                        sling:resourceType="granite/ui/components/coral/foundation/include"/>
                    <pageAnalytics
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Page Analytics"
                        path="/apps/aemdesign/global/dialog/touch/pageAnalytics_design/content"
                        resourceType="granite/ui/components/coral/foundation/container"
                        sling:resourceType="granite/ui/components/coral/foundation/include"/>
                </items>
            </plugins>
        </items>
    </content>
</jcr:root>
