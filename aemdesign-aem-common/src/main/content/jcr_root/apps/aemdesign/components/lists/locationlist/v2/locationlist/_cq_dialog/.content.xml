<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Location List"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
        <layout
                jcr:primaryType="nt:unstructured"
                type="nav"
                sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">

            <listTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="List"
                    path="/apps/aemdesign/components/lists/list/v2/list/cq:dialog/content/items/listTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>

            <listConfigTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="List Config"
                    path="/apps/aemdesign/components/lists/list/v2/list/cq:dialog/content/items/listConfigTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>

            <layoutTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Layout"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Layout"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <detailsBadge
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldDescription="Which badge to select from the Details component on the page"
                                            fieldLabel="Use Badge"
                                            name="./detailsBadge"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/components/lists/locationlist/badge"/>
                                    </detailsBadge>
                                    <topic
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldDescription="Subscribers waiting for notification of the filter value is updated"
                                            fieldLabel="Topic Queue"
                                            name="./topicQueue"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-topic/topic"/>
                                    </topic>
                                </items>
                            </variant>
                        </items>
                    </column>
                </items>
            </layoutTab>

            <mapBackgroundTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Map Background"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <attributes
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Thumbnail Asset"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <mapBackground
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                            autoStart="{Boolean}false"
                                            class="cq-droptarget"
                                            fileNameParameter="./map/fileName"
                                            fileReferenceParameter="./map/fileReference"
                                            mimeTypes="[image]"
                                            multiple="{Boolean}false"
                                            name="./map/file"
                                            uploadUrl="${suffix.path}"
                                            allowUpload="{Boolean}false"
                                            cropParameter="./map/imageCrop"
                                            mapParameter="./map/imageMap"
                                            requestSuffix=".img.png"
                                            rotateParameter="./map/imageRotate"
                                            useHTML5="{Boolean}true"/>

                                    <mapBackgroundResourceType
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/hidden"
                                            name="./map/sling:resourceType"
                                            value="aemdesign/components/media/image"/>
                                </items>
                            </attributes>
                            <mapBackgroundStyle
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Map Background Config"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <mapRenderingOption
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            emptyOption="{Boolean}true"
                                            fieldDescription="Pick a rendering option for this image."
                                            fieldLabel="Rendering Option"
                                            name="./mapRenderingOption"
                                            value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Default"
                                                    value=""/>
                                            <icon
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Icon"
                                                    value="icon"/>
                                            <dam
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Asset DAM Rendition"
                                                    value="dam"/>
                                            <customdam
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Thumbnail DAM Rendition"
                                                    value="customdam"/>
                                            <custom
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Custom Thumbnail Rendition"
                                                    value="custom"/>
                                        </items>
                                    </mapRenderingOption>
                                </items>
                            </mapBackgroundStyle>
                        </items>
                    </column>
                </items>
            </mapBackgroundTab>


            <badgeConfigTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Badge Config"
                    path="/apps/aemdesign/global/dialog/touch/badge/content/items/badgeConfigTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>

            <badgeThumbnailTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Badge Thumbnail"
                    path="/apps/aemdesign/global/dialog/touch/asset/content/items/badgeThumbnailTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>


            <styleTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Style"
                    path="/apps/aemdesign/global/dialog/touch/style/content/items/styleTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
            <accessibilityTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Accessibility"
                    path="/apps/aemdesign/global/dialog/touch/accessibility/content/items/accessibilityTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
        </items>
    </content>

</jcr:root>
