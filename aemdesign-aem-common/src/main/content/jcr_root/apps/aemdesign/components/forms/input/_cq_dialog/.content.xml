<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Input"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
        <layout
                jcr:primaryType="nt:unstructured"
                type="nav"
                sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">

            <basicTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Basic"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <name
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Element Name"
                                    name="./name"
                                    required="{Boolean}true"/>
                            <title
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Title"
                                    name="./jcr:title"/>
                            <hidetitle
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    fieldLabel="Hide Title"
                                    name="./hideTitle"
                                    text="Hide Title"
                                    value="true"/>
                            <description
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Description"
                                    name="./jcr:description"/>
                            <type
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/select"
                                    fieldLabel="Type"
                                    name="./type"
                                    value="">
                                <items jcr:primaryType="nt:unstructured">
                                    <default
                                            jcr:primaryType="nt:unstructured"
                                            text="Default"
                                            value=""/>
                                    <text
                                            jcr:primaryType="nt:unstructured"
                                            text="Text"
                                            value="text"/>
                                    <email
                                            jcr:primaryType="nt:unstructured"
                                            text="Email"
                                            value="email"/>
                                    <hidden
                                            jcr:primaryType="nt:unstructured"
                                            text="Hidden"
                                            value="hidden"/>
                                    <submit
                                            jcr:primaryType="nt:unstructured"
                                            text="Submit"
                                            value="submit"/>
                                </items>
                            </type>
                            <placeHolder
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Place Holder"
                                    name="./placeHolder"/>
                            <value
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Default Value"
                                    name="./defaultValue"/>
                            <required
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                    fieldLabel="Required"
                                    name="./required"
                                    text="Required"
                                    value="true"/>
                            <requiredMessage
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                    fieldLabel="Required Message"
                                    name="./requiredMessage"/>
                            <constraintType
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/foundation/form/select"
                                    fieldLabel="Constraint"
                                    name="./constraintType">
                                <items jcr:primaryType="nt:unstructured"/>
                            </constraintType>
                            <constraintMessage
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                    fieldLabel="Constraint Message"
                                    name="./constraintMessage"/>
                            <multiLine
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="cq/gui/components/authoring/dialog/sizefield"
                                    fieldLabel="Size">
                                <width
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        name="./cols"
                                        unit="cols"/>
                                <height
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        name="./rows"
                                        unit="rows"/>
                            </multiLine>
                            <width
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="Width"
                                    name="./width"/>
                            <class
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                    fieldLabel="CSS"
                                    name="./css"/>
                        </items>
                    </column>
                </items>
            </basicTab>

            <layoutTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Layout"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Layout"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <variant
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            emptyOption="{Boolean}true"
                                            fieldLabel="Variant"
                                            name="./variant"
                                            value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Default"
                                                    value=""/>
                                        </items>
                                    </variant>
                                </items>
                            </variant>
                        </items>
                    </column>
                </items>
            </layoutTab>

            <styleTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Style"
                    path="/apps/aemdesign/global/dialog/touch/style/content/items/styleTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
            <accessibilityTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Accessibility"
                    path="/apps/aemdesign/global/dialog/touch/accessibility/content/items/accessibilityTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
            <analyticsTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Analytics"
                    path="/apps/aemdesign/global/dialog/touch/analytics/content/items/analyticsTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
        </items>
    </content>

</jcr:root>
