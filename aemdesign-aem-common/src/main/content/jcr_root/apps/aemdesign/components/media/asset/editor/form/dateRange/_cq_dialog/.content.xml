<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <dateRange
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Field Title"
                                        name="./title"/>
                                    <fromDateField
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Namespace:localpart such as 'dam:startDate'.If no inputted is entered, this field will not be displayed"
                                        fieldLabel="From Date Field Name"
                                        name="./fromDateField"/>
                                    <fromDatePattern
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Default pattern is 'yyyy'. "
                                        fieldLabel="From Date Pattern"
                                        name="./fromDatePattern"/>
                                    <toDateField
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Namespace:localpart such as 'dam:finishDate'.If no inputted is entered, this field will not be displayed"
                                        fieldLabel="To Date Field Name"
                                        name="./toDateField"/>
                                    <toDatePattern
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Default pattern is 'yyyy' "
                                        fieldLabel="To Date Pattern"
                                        name="./toDatePattern"/>
                                    <seperator
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Default separator is '-' "
                                        fieldLabel="Separator"
                                        name="./separator"
                                        required="{Boolean}false"/>
                                </items>
                            </column>
                        </items>
                    </dateRange>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
