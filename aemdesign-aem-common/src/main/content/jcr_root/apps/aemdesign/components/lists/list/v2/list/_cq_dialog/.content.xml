<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="List"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
        <layout
                jcr:primaryType="nt:unstructured"
                type="nav"
                sling:resourceType="granite/ui/components/foundation/layouts/tabs"/>
        <items jcr:primaryType="nt:unstructured">


            <listTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="List"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column1
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="List"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <listFrom
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            class="cq-dialog-dropdown-showhide"
                                            cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                            fieldLabel="Build list using"
                                            emptyOption="{Boolean}true"
                                            name="./listFrom">
                                        <items jcr:primaryType="nt:unstructured">
                                            <children
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Child pages"
                                                    value="children"/>
                                            <descendants
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Descendant pages"
                                                    value="descendants"/>
                                            <static
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Fixed list"
                                                    value="static"/>
                                            <search
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Search"
                                                    value="search"/>
                                            <querybuilder
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Advanced Search"
                                                    value="querybuilder"/>
                                            <tags
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Tags"
                                                    value="tags"/>
                                        </items>
                                    </listFrom>
                                    <setChildren
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container"
                                            class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                            showhidetargetvalue="children">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/heading"
                                                    class="coral-Heading coral-Heading--4"
                                                    level="{Long}4"
                                                    text="Options for Child Pages"/>
                                            <well
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <parentPage
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            fieldDescription="Leave empty to use current page"
                                                            fieldLabel="Parent page"
                                                            name="./parentPage"
                                                            rootPath="/content"/>
                                                </items>
                                            </well>
                                        </items>
                                    </setChildren>
                                    <setDescendants
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container"
                                            class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                            showhidetargetvalue="descendants">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/heading"
                                                    class="coral-Heading coral-Heading--4"
                                                    level="{Long}4"
                                                    text="Options for Descendant Pages"/>
                                            <well
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <parentPage
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            fieldDescription="Leave empty to use current page"
                                                            fieldLabel="Descendants of"
                                                            name="./ancestorPage"
                                                            rootPath="/content"/>
                                                </items>
                                            </well>
                                        </items>
                                    </setDescendants>
                                    <setStatic
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container"
                                            class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                            showhidetargetvalue="static">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/heading"
                                                    class="coral-Heading coral-Heading--4"
                                                    level="{Long}4"
                                                    text="Options for Fixed list"/>
                                            <well
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pages
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                            class="foundation-layout-util-maximized-alt">
                                                        <field
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                                name="./pages"
                                                                rootPath="/content"/>
                                                    </pages>
                                                </items>
                                            </well>
                                        </items>
                                    </setStatic>
                                    <setSearch
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container"
                                            class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                            showhidetargetvalue="search">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/heading"
                                                    class="coral-Heading coral-Heading--4"
                                                    level="{Long}4"
                                                    text="Options for Search"/>
                                            <well
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <searchIn
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            fieldDescription="Leave empty to use current site (eg. /content/mysite)"
                                                            fieldLabel="Search in"
                                                            name="./searchIn"
                                                            rootPath="/content"/>
                                                    <query
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                            fieldLabel="Search query"
                                                            name="./query"/>
                                                </items>
                                            </well>
                                        </items>
                                    </setSearch>
                                    <setQuerybuilder
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container"
                                            class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                            showhidetargetvalue="querybuilder">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/heading"
                                                    class="coral-Heading coral-Heading--4"
                                                    level="{Long}4"
                                                    text="Options for Advanced search"/>
                                            <well
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <savedquery
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/textarea"
                                                            class="foundation-layout-util-maximized-alt"
                                                            fieldLabel="Querybuilder predicate notation"
                                                            name="./savedquery"/>
                                                </items>
                                            </well>
                                        </items>
                                    </setQuerybuilder>
                                    <setTags
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/container"
                                            class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                            showhidetargetvalue="tags">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/heading"
                                                    class="coral-Heading coral-Heading--4"
                                                    level="{Long}4"
                                                    text="Options for Tags"/>
                                            <well
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <parentPage
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            fieldDescription="Leave empty to use current page"
                                                            fieldLabel="Parent page"
                                                            name="./tagsSearchRoot"
                                                            rootPath="/content"/>
                                                    <tags
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="cq/gui/components/common/tagspicker"
                                                            fieldLabel="Tags"
                                                            name="./tags"/>
                                                    <match
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                                            fieldLabel="Match"
                                                            name="./tagsMatch">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <any
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="any tag"
                                                                    value="any"/>
                                                            <all
                                                                    jcr:primaryType="nt:unstructured"
                                                                    text="all tags"
                                                                    value="all"/>
                                                        </items>
                                                    </match>
                                                </items>
                                            </well>
                                        </items>
                                    </setTags>
                                </items>
                            </basic>
                        </items>
                    </column1>
                    <column2
                            jcr:primaryType="nt:unstructured"
                            class="u-coral-padding u-coral-noMargin"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Config"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <orderBy
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldDescription="Enter a property like 'jcr:created'"
                                            fieldLabel="Order by"
                                            emptyOption="{Boolean}true"
                                            name="./orderBy">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/components/lists/list/orderby"/>
                                    </orderBy>
                                    <limit
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                            fieldDescription="Maximum number of items displayed in list"
                                            fieldLabel="Limit"
                                            name="./limit"/>
                                    <pagination
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                            fieldLabel="Paginate after"
                                            name="./pageMax"/>
                                    <paginationType
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldLabel="Pagination Type"
                                            name="./paginationType"
                                            value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Default"
                                                    value=""/>
                                            <hidden
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Hidden"
                                                    value="hidden"/>
                                            <prev_next_pages
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Previous and Next Pages"
                                                    value="default"/>
                                        </items>
                                    </paginationType>
                                </items>
                            </basic>
                        </items>
                    </column2>
                </items>
            </listTab>

            <listConfigTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="List Config"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Config"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <printStructure
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldDescription="Don't print structure of the list, omit (OL/UL and LI tags)"
                                            fieldLabel="Print List Structure"
                                            name="./printStructure"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"/>
                                    </printStructure>
                                    <showHidden
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldDescription="pages with selected Hide in Navigation will be hidden"
                                            fieldLabel="Show Hidden Pages"
                                            name="./showHidden"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"/>
                                    </showHidden>
                                    <listSplit
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldDescription="break list after number of items"
                                            fieldLabel="Split list into section"
                                            name="./listSplit"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"/>
                                    </listSplit>
                                    <listSplitEvery
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                            fieldDescription="break list after this number of items"
                                            fieldLabel="Split List After Every"
                                            name="./listSplitEvery"/>

                                </items>
                            </basic>
                            <feed
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Feed"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <feedEnabled
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldLabel="Enable Feed"
                                            name="./feedEnabled"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"/>
                                    </feedEnabled>
                                    <feedType
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldDescription="type of feed to show as icon"
                                            fieldLabel="Feed Type"
                                            name="./feedType"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Default"
                                                    value=""/>
                                            <rss
                                                    jcr:primaryType="nt:unstructured"
                                                    text="RSS"
                                                    value="rss"/>
                                            <atom
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Atom"
                                                    value="atom"/>
                                        </items>
                                    </feedType>

                                </items>
                            </feed>
                        </items>
                    </column>
                </items>
            </listConfigTab>


            <layoutTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Layout"
                    sling:resourceType="granite/ui/components/foundation/section">

                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Layout"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <detailsBadge
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldDescription="Which badge to select from the Details component on the page"
                                            fieldLabel="Use Badge"
                                            name="./detailsBadge"
                                            emptyOption="{Boolean}true"
                                            value="">
                                        <datasource
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                variant="valuelist"
                                                path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/components/lists/list/badge"/>
                                    </detailsBadge>
                                </items>
                            </variant>
                        </items>
                    </column>
                </items>
            </layoutTab>


            <badgeConfigTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Badge Config"
                    path="/apps/aemdesign/global/dialog/touch/badge/content/items/badgeConfigTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>

            <badgeThumbnailTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Badge Thumbnail"
                    path="/apps/aemdesign/global/dialog/touch/asset/content/items/badgeThumbnailTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>


            <styleTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Style"
                    path="/apps/aemdesign/global/dialog/touch/style/content/items/styleTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
            <accessibilityTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Accessibility"
                    path="/apps/aemdesign/global/dialog/touch/accessibility/content/items/accessibilityTab"
                    resourceType="granite/ui/components/foundation/section"
                    sling:resourceType="granite/ui/components/coral/foundation/include"/>
        </items>
    </content>

</jcr:root>
