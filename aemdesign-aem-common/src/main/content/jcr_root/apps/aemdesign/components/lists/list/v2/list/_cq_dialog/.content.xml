<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="List"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="/mnt/overlay/wcm/core/content/sites/components/details.html/apps/aemdesign/components/lists/list/v2/list">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <listTab
                jcr:primaryType="nt:unstructured"
                jcr:title="List"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                jcr:primaryType="nt:unstructured"
                                jcr:title="List"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">

                                    <listFrom
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        class="cq-dialog-dropdown-showhide"
                                        cq-dialog-dropdown-showhide-target=".list-option-listfrom-showhide-target"
                                        emptyOption="{Boolean}true"
                                        fieldLabel="Build list using"
                                        name="./listFrom">>
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            variant="valuelist"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/components/lists/list/listfrom"/>
                                    </listFrom>

                                    <setChildren
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="children">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for Child Pages"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <parentPage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                        fieldDescription="Leave empty to use current page"
                                                        fieldLabel="Parent page"
                                                        name="./parentPage"
                                                        rootPath="/content"/>
                                                </items>
                                            </well>
                                        </items>
                                    </setChildren>
                                    <setDescendants
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="descendants">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for Descendant Pages"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <parentPage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                        fieldDescription="Leave empty to use current page"
                                                        fieldLabel="Descendants of"
                                                        name="./ancestorPage"
                                                        rootPath="/content"/>
                                                </items>
                                            </well>
                                        </items>
                                    </setDescendants>
                                    <setStatic
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="static">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for Fixed list"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <pages
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                        class="foundation-layout-util-maximized-alt">
                                                        <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            name="./pages"
                                                            rootPath="/content"/>
                                                    </pages>
                                                </items>
                                            </well>
                                        </items>
                                    </setStatic>
                                    <setSearch
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="search">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for Search"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <searchIn
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                        fieldDescription="Leave empty to use current site (eg. /content/mysite)"
                                                        fieldLabel="Search in"
                                                        name="./searchIn"
                                                        rootPath="/content"/>
                                                    <query
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                        fieldLabel="Search query"
                                                        name="./query"/>
                                                </items>
                                            </well>
                                        </items>
                                    </setSearch>
                                    <setQuerybuilder
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="querybuilder">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for Advanced search"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <savedquery
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/textarea"
                                                        class="foundation-layout-util-maximized-alt"
                                                        fieldLabel="Querybuilder predicate notation"
                                                        name="./savedquery"/>
                                                </items>
                                            </well>
                                        </items>
                                    </setQuerybuilder>
                                    <setTags
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listfrom-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="tags">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for Tags"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <parentPage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                        fieldDescription="Leave empty to use current page"
                                                        fieldLabel="Parent page"
                                                        name="./tagsSearchRoot"
                                                        rootPath="/content"/>
                                                    <tags
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/common/tagspicker"
                                                        fieldLabel="Tags"
                                                        name="./tags"/>
                                                    <match
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                                        fieldLabel="Match"
                                                        name="./tagsMatch">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <any
                                                                jcr:primaryType="nt:unstructured"
                                                                text="any tag"
                                                                value="any"/>
                                                            <all
                                                                jcr:primaryType="nt:unstructured"
                                                                text="all tags"
                                                                value="all"/>
                                                        </items>
                                                    </match>
                                                </items>
                                            </well>
                                        </items>
                                    </setTags>
                                </items>
                            </basic>
                        </items>
                    </column1>
                    <column2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container"
                        class="u-coral-padding u-coral-noMargin">
                        <granite:rendercondition
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/not">
                            <granite:rendercondition
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="aemdesign/components/coral/renderconditions/iscurrentuseringroup"
                                groups="[content-authors-advanced]"/>
                        </granite:rendercondition>
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Config"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <orderBy
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        emptyOption="{Boolean}true"
                                        fieldDescription="Enter a property like 'jcr:created'"
                                        fieldLabel="Order by"
                                        name="./orderBy">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/components/lists/list/orderby"
                                            variant="valuelist"/>
                                    </orderBy>
                                    <sortOrder
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="Sort order ascending or descending"
                                        fieldLabel="Sort Order"
                                        name="./sortOrder"
                                        type="editable">
                                        <items jcr:primaryType="nt:unstructured">
                                            <asc
                                                jcr:primaryType="nt:unstructured"
                                                text="Ascending"
                                                value="asc"/>
                                            <desc
                                                jcr:primaryType="nt:unstructured"
                                                text="Descending"
                                                value="desc"/>
                                        </items>
                                    </sortOrder>
                                    <limit
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldDescription="Maximum number of items displayed in list"
                                        fieldLabel="Limit"
                                        name="./limit"/>
                                    <pagination
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldLabel="Paginate after"
                                        name="./pageMax"/>
                                    <paginationType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldLabel="Pagination Type"
                                        name="./paginationType"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                jcr:primaryType="nt:unstructured"
                                                text="Default"
                                                value=""/>
                                            <hidden
                                                jcr:primaryType="nt:unstructured"
                                                text="Hidden"
                                                value="hidden"/>
                                            <prev_next_pages
                                                jcr:primaryType="nt:unstructured"
                                                text="Previous and Next Pages"
                                                value="default"/>
                                            <pages
                                                jcr:primaryType="nt:unstructured"
                                                text="Pages"
                                                value="pages"/>
                                        </items>
                                    </paginationType>
                                    <headerType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldLabel="Header Type"
                                        name="./headerType"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <none
                                                jcr:primaryType="nt:unstructured"
                                                text="None"
                                                value=""/>
                                            <default
                                                jcr:primaryType="nt:unstructured"
                                                text="Showing S-C of X results"
                                                value="default"/>
                                            <hidden
                                                jcr:primaryType="nt:unstructured"
                                                text="Hidden"
                                                value="hidden"/>
                                        </items>
                                    </headerType>
                                </items>
                            </basic>
                        </items>
                    </column2>
                </items>
            </listTab>
            <listConfigTab
                jcr:primaryType="nt:unstructured"
                jcr:title="List Config"
                sling:resourceType="granite/ui/components/foundation/section">
                <granite:rendercondition
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/renderconditions/not">
                    <granite:rendercondition
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/coral/renderconditions/iscurrentuseringroup"
                        groups="[content-authors-advanced]"/>
                </granite:rendercondition>
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Config"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <printStructure
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        emptyOption="{Boolean}true"
                                        fieldDescription="Don't print structure of the list, omit (OL/UL and LI tags)"
                                        fieldLabel="Print List Structure"
                                        name="./printStructure"
                                        value="">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"
                                            variant="valuelist"/>
                                    </printStructure>
                                    <showHidden
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        emptyOption="{Boolean}true"
                                        fieldDescription="pages with selected Hide in Navigation will be hidden"
                                        fieldLabel="Show Hidden Pages"
                                        name="./showHidden"
                                        value="">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"
                                            variant="valuelist"/>
                                    </showHidden>
                                    <listSplit
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        emptyOption="{Boolean}true"
                                        fieldDescription="break list after number of items"
                                        fieldLabel="Split list into section"
                                        name="./listSplit"
                                        value="">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"
                                            variant="valuelist"/>
                                    </listSplit>
                                    <listSplitEvery
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldDescription="break list after this number of items"
                                        fieldLabel="Split List After Every"
                                        name="./listSplitEvery"/>
                                </items>
                            </basic>
                            <feed
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Feed"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <feedEnabled
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        emptyOption="{Boolean}true"
                                        fieldLabel="Enable Feed"
                                        name="./feedEnabled"
                                        value="">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"
                                            variant="valuelist"/>
                                    </feedEnabled>
                                    <feedType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        emptyOption="{Boolean}true"
                                        fieldDescription="type of feed to show as icon"
                                        fieldLabel="Feed Type"
                                        name="./feedType"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                jcr:primaryType="nt:unstructured"
                                                text="Default"
                                                value=""/>
                                            <rss
                                                jcr:primaryType="nt:unstructured"
                                                text="RSS"
                                                value="rss"/>
                                            <atom
                                                jcr:primaryType="nt:unstructured"
                                                text="Atom"
                                                value="atom"/>
                                        </items>
                                    </feedType>
                                </items>
                            </feed>
                        </items>
                    </column>
                </items>
            </listConfigTab>
            <layoutTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Layout"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                    margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Layout"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <detailsBadge
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        emptyOption="{Boolean}true"
                                        fieldDescription="Which badge to select from the Details component on the page"
                                        fieldLabel="Use Badge"
                                        name="./detailsBadge"
                                        value="">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/components/lists/list/badge"
                                            variant="valuelist"/>
                                    </detailsBadge>
                                </items>
                            </variant>
                        </items>
                    </column>
                </items>
            </layoutTab>
            <badgeConfigTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Badge Config"
                sling:resourceType="granite/ui/components/coral/foundation/include"
                path="/apps/aemdesign/global/dialog/touch/badge/content/items/badgeConfigTab"
                resourceType="granite/ui/components/foundation/section"/>
            <badgeThumbnailTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Badge Thumbnail"
                sling:resourceType="granite/ui/components/coral/foundation/include"
                path="/apps/aemdesign/global/dialog/touch/asset/content/items/badgeThumbnailTab"
                resourceType="granite/ui/components/foundation/section"/>
            <styleTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Style"
                sling:resourceType="granite/ui/components/coral/foundation/include"
                path="/apps/aemdesign/global/dialog/touch/style/content/items/styleTab"
                resourceType="granite/ui/components/foundation/section"/>
            <accessibilityTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Accessibility"
                sling:resourceType="granite/ui/components/coral/foundation/include"
                path="/apps/aemdesign/global/dialog/touch/accessibility/content/items/accessibilityTab"
                resourceType="granite/ui/components/foundation/section"/>
            <analyticsTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Analytics"
                sling:resourceType="granite/ui/components/coral/foundation/include"
                path="/apps/aemdesign/global/dialog/touch/analytics/content/items/analyticsTab"
                resourceType="granite/ui/components/foundation/section"/>
        </items>
    </content>
</jcr:root>
