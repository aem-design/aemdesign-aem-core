<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="List"
          helpPath="/mnt/overlay/wcm/core/content/sites/components/details.html/apps/aemdesign/components/lists/list/v2/list"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>

        <items jcr:primaryType="nt:unstructured">
            <listTab
                jcr:primaryType="nt:unstructured"
                jcr:title="List"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                        margin="{Boolean}false"/>

                <items jcr:primaryType="nt:unstructured">
                    <column1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                jcr:primaryType="nt:unstructured"
                                jcr:title="List"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <listSource
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        class="cq-dialog-dropdown-showhide"
                                        cq-dialog-dropdown-showhide-target=".list-option-listsource-showhide-target"
                                        fieldLabel="List Source"
                                        emptyOption="{Boolean}false"
                                        name="./listFrom">
                                        <items jcr:primaryType="nt:unstructured">
                                            <children
                                                jcr:primaryType="nt:unstructured"
                                                text="Children"
                                                value="children"/>
                                            <descendants
                                                jcr:primaryType="nt:unstructured"
                                                text="Descendants"
                                                value="descendants"/>
                                            <static
                                                jcr:primaryType="nt:unstructured"
                                                text="Static"
                                                value="static"/>
                                            <search
                                                jcr:primaryType="nt:unstructured"
                                                text="Search"
                                                value="search"/>
                                            <tags
                                                jcr:primaryType="nt:unstructured"
                                                text="Tags"
                                                value="tags"/>
                                            <querybuilder
                                                jcr:primaryType="nt:unstructured"
                                                text="Advanced Search"
                                                value="querybuilder"/>
                                        </items>
                                    </listSource>

                                    <listSourceChildren
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listsource-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="children">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for child pages"/>

                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>

                                                <items jcr:primaryType="nt:unstructured">
                                                    <parentPage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                        fieldDescription="Define a path to find child pages within, leave this field empty to use the current path."
                                                        fieldLabel="Parent page"
                                                        name="./parentPage"
                                                        rootPath="/content"/>
                                                </items>
                                            </well>
                                        </items>
                                    </listSourceChildren>

                                    <listSourceDescendants
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listsource-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="descendants">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for descendant pages"/>

                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>
                                                <items jcr:primaryType="nt:unstructured">
                                                    <parentPage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                        fieldDescription="Define a path to find descendant pages, leave this field empty to use the current path."
                                                        fieldLabel="Descendants Path"
                                                        name="./ancestorPage"
                                                        rootPath="/content"/>
                                                </items>
                                            </well>
                                        </items>
                                    </listSourceDescendants>

                                    <listSourceStatic
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listsource-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="static">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for static list"/>

                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>

                                                <items jcr:primaryType="nt:unstructured">
                                                    <pages
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/multifield"
                                                        class="foundation-layout-util-maximized-alt">
                                                        <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                            name="./pages"
                                                            rootPath="/content"/>
                                                    </pages>
                                                </items>
                                            </well>
                                        </items>
                                    </listSourceStatic>

                                    <listSourceSearch
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listsource-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="search">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for query search"/>

                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>

                                                <items jcr:primaryType="nt:unstructured">
                                                    <searchIn
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                        fieldDescription="Leave empty to use current site (eg. /content/mysite)"
                                                        fieldLabel="Search Context"
                                                        name="./searchIn"
                                                        rootPath="/content"/>

                                                    <query
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                                        fieldLabel="Search Query"
                                                        name="./query"/>
                                                </items>
                                            </well>
                                        </items>
                                    </listSourceSearch>

                                    <listSourceTags
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listsource-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="tags">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for tags search"/>

                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>

                                                <items jcr:primaryType="nt:unstructured">
                                                    <parentPage
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                                        fieldDescription="Leave empty to use current page"
                                                        fieldLabel="Parent Page"
                                                        name="./tagsSearchRoot"
                                                        rootPath="/content"/>

                                                    <tags
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/common/tagspicker"
                                                        fieldLabel="Tags"
                                                        name="./tags"/>

                                                    <match
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                                        fieldLabel="Match Type"
                                                        name="./tagsMatch">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <any
                                                                jcr:primaryType="nt:unstructured"
                                                                text="any tag"
                                                                value="any"/>
                                                            <all
                                                                jcr:primaryType="nt:unstructured"
                                                                text="all tags"
                                                                value="all"/>
                                                        </items>
                                                    </match>

                                                    <useDetailsTags
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                        fieldDescription="Choose whether or not the cq:tags property on the details component should be searched too."
                                                        fieldLabel="Use Details Tags"
                                                        name="./useDetailsTags"
                                                        emptyOption="{Boolean}true"
                                                        value="">
                                                        <datasource
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                                            variant="valuelist"
                                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"/>
                                                    </useDetailsTags>
                                                </items>
                                            </well>
                                        </items>
                                    </listSourceTags>

                                    <listSourceQuerybuilder
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/container"
                                        class="hide list-option-listsource-showhide-target foundation-layout-util-vmargin"
                                        showhidetargetvalue="querybuilder">
                                        <items jcr:primaryType="nt:unstructured">
                                            <heading
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/heading"
                                                class="coral-Heading coral-Heading--4"
                                                level="{Long}4"
                                                text="Options for Advanced search"/>

                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/foundation/container">
                                                <layout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/layouts/well"/>

                                                <items jcr:primaryType="nt:unstructured">
                                                    <savedquery
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/form/textarea"
                                                        class="foundation-layout-util-maximized-alt"
                                                        fieldLabel="Query Builder Predicate"
                                                        name="./savedquery"/>
                                                </items>
                                            </well>
                                        </items>
                                    </listSourceQuerybuilder>
                                </items>
                            </basic>
                        </items>
                    </column1>

                    <column2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container"
                        class="u-coral-padding u-coral-noMargin">
                        <items jcr:primaryType="nt:unstructured">
                            <listOptions
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Options"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <orderBy
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="Define how the results will be sorted when queried, this doesn't apply to static lists."
                                        fieldLabel="Order By"
                                        emptyOption="{Boolean}true"
                                        name="./orderBy">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            variant="valuelist"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/components/lists/list/orderby"/>
                                    </orderBy>

                                    <sortOrder
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldDescription="Sort order ascending or descending"
                                        fieldLabel="Sort Order"
                                        name="./sortOrder"
                                        type="editable">
                                        <items jcr:primaryType="nt:unstructured">
                                            <asc
                                                jcr:primaryType="nt:unstructured"
                                                text="Ascending"
                                                value="asc"/>
                                            <desc
                                                jcr:primaryType="nt:unstructured"
                                                text="Descending"
                                                value="desc"/>
                                        </items>
                                    </sortOrder>

                                    <limit
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldDescription="Maximum number of items displayed in list."
                                        fieldLabel="Limit"
                                        name="./limit"/>

                                    <pagination
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldDescription="Set the number of results before pagination will appear."
                                        fieldLabel="Paginate After"
                                        name="./pageMax"/>

                                    <paginationType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldLabel="Pagination Type"
                                        name="./paginationType"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                jcr:primaryType="nt:unstructured"
                                                text="Default"
                                                value=""/>
                                            <hidden
                                                jcr:primaryType="nt:unstructured"
                                                text="Hidden"
                                                value="hidden"/>
                                            <prev_next_pages
                                                jcr:primaryType="nt:unstructured"
                                                text="Previous and Next Pages"
                                                value="default"/>
                                        </items>
                                    </paginationType>
                                </items>
                            </listOptions>
                        </items>
                    </column2>
                </items>
            </listTab>

            <listConfigTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Configuration"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout jcr:primaryType="nt:unstructured"
                        margin="{Boolean}false"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>

                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <basic
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Config"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <printStructure
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="When 'false', the HTML structure won't be generated around each result."
                                        fieldLabel="Print List Structure"
                                        name="./printStructure"
                                        emptyOption="{Boolean}true"
                                        value="">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            variant="valuelist"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"/>
                                    </printStructure>

                                    <showHidden
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="Define whether or not pages hidden from navigation will be visible in the list."
                                        fieldLabel="Show Hidden Pages"
                                        name="./showHidden"
                                        emptyOption="{Boolean}true"
                                        value="">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            defaultSelection="true"
                                            variant="valuelist"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"/>
                                    </showHidden>

                                    <listSplit
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="Toggle the results so they are broken into sets of results instead of one long list."
                                        fieldLabel="Split Sections"
                                        name="./listSplit"
                                        emptyOption="{Boolean}true"
                                        value="">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            variant="valuelist"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"/>
                                    </listSplit>

                                    <listSplitEvery
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        fieldDescription="How many results should occur before the section split occurs?"
                                        fieldLabel="Split Sections Every"
                                        name="./listSplitEvery"/>
                                </items>
                            </basic>

                            <feed
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Feed"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <feedEnabled
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldLabel="Enable Feed"
                                        name="./feedEnabled"
                                        emptyOption="{Boolean}true"
                                        value="">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            variant="valuelist"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/common/true-false"/>
                                    </feedEnabled>

                                    <feedType
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="type of feed to show as icon"
                                        fieldLabel="Feed Type"
                                        name="./feedType"
                                        emptyOption="{Boolean}true"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                jcr:primaryType="nt:unstructured"
                                                text="Default"
                                                value=""/>
                                            <rss
                                                jcr:primaryType="nt:unstructured"
                                                text="RSS"
                                                value="rss"/>
                                            <atom
                                                jcr:primaryType="nt:unstructured"
                                                text="Atom"
                                                value="atom"/>
                                        </items>
                                    </feedType>
                                </items>
                            </feed>
                        </items>
                    </column>
                </items>
            </listConfigTab>

            <layoutTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Layout"
                sling:resourceType="granite/ui/components/foundation/section">
                <layout jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                        margin="{Boolean}false"/>

                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Layout"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <detailsBadge
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="Which badge template should be used when generating each result?"
                                        fieldLabel="Render Badge"
                                        name="./detailsBadge"
                                        emptyOption="{Boolean}true"
                                        value="">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="aemdesign/components/coral/datasources/tags"
                                            variant="valuelist"
                                            path="/content/cq:tags/${(empty tenant.id and empty tenantId) ? 'aemdesign' : (empty tenant.id ? tenantId : tenant.id )}/component-dialog/components/lists/list/badge"/>
                                    </detailsBadge>
                                </items>
                            </variant>
                        </items>
                    </column>
                </items>
            </layoutTab>

            <badgeConfigTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Badge Config"
                path="/apps/aemdesign/global/dialog/touch/badge/content/items/badgeConfigTab"
                resourceType="granite/ui/components/foundation/section"
                sling:resourceType="granite/ui/components/coral/foundation/include"/>

            <badgeThumbnailTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Badge Thumbnail"
                path="/apps/aemdesign/global/dialog/touch/asset/content/items/badgeThumbnailTab"
                resourceType="granite/ui/components/foundation/section"
                sling:resourceType="granite/ui/components/coral/foundation/include"/>

            <styleTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Style"
                path="/apps/aemdesign/global/dialog/touch/style/content/items/styleTab"
                resourceType="granite/ui/components/foundation/section"
                sling:resourceType="granite/ui/components/coral/foundation/include"/>

            <accessibilityTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Accessibility"
                path="/apps/aemdesign/global/dialog/touch/accessibility/content/items/accessibilityTab"
                resourceType="granite/ui/components/foundation/section"
                sling:resourceType="granite/ui/components/coral/foundation/include"/>

            <analyticsTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Analytics"
                path="/apps/aemdesign/global/dialog/touch/analytics/content/items/analyticsTab"
                resourceType="granite/ui/components/foundation/section"
                sling:resourceType="granite/ui/components/coral/foundation/include"/>
        </items>
    </content>
</jcr:root>
