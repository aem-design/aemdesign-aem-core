<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Gallery"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/tabs">
        <items jcr:primaryType="nt:unstructured">
            <listTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Basic"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <listFrom
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Build list using"
                                name="./listFrom"
                                value="">
                                <items jcr:primaryType="nt:unstructured">
                                    <children
                                        jcr:primaryType="nt:unstructured"
                                        text="Child pages"
                                        value="children"/>
                                    <static
                                        jcr:primaryType="nt:unstructured"
                                        text="Fixed list"
                                        value="static"/>
                                    <damCollection
                                        jcr:primaryType="nt:unstructured"
                                        text="DAM Collection"
                                        value="collection"/>
                                </items>
                            </listFrom>
                        </items>
                    </column>
                </items>
            </listTab>
            <childrenTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Child pages"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <listPath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="Leave empty to use current page"
                                fieldLabel="Parent page"
                                name="./listPath"
                                rootPath="/content"/>
                        </items>
                    </column>
                </items>
            </childrenTab>
            <staticTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Fixed list"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <listItems
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                fieldDescription="Click the '+' to add a new page"
                                fieldLabel="Items">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                    name="./listItems"
                                    required="{Boolean}false"/>
                            </listItems>
                        </items>
                    </column>
                </items>
            </staticTab>
            <damCollectionTab
                jcr:primaryType="nt:unstructured"
                jcr:title="DAM Collection"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <listCollections
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Collection"
                                name="./collection"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </listCollections>
                            <assetViewerPage
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldDescription="The location of the Asset Viewer Page to use"
                                fieldLabel="Asset Viewer Page Path"
                                name="./assetViewerPagePath"
                                rootPath="/content"/>
                        </items>
                    </column>
                </items>
            </damCollectionTab>
            <layout
                jcr:primaryType="nt:unstructured"
                jcr:title="Layout"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <lightbox_size
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/sizefield"
                                fieldDescription="Note: Max width and height will be crop at 70%. If these fields have no value then it will resize to original size of picture. The content display size in lightbox of a page, which it is determined by the the first video or media gallery from the page."
                                fieldLabel="Lightbox Media Display Area">
                                <width
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                    name="./lightboxWidth"
                                    unit="%"/>
                                <height
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                    name="./lightboxHeight"
                                    unit="%"/>
                            </lightbox_size>
                            <titleAltPrefixText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Prefix text for hover"
                                name="./titleAltPrefixText"/>
                            <variant
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldLabel="Variant"
                                name="./variant"
                                value="">
                                <items jcr:primaryType="nt:unstructured">
                                    <default
                                        jcr:primaryType="nt:unstructured"
                                        text="Default"
                                        value=""/>
                                    <slider
                                        jcr:primaryType="nt:unstructured"
                                        text="Slider"
                                        value="default"/>
                                    <montageAssetViewer
                                        jcr:primaryType="nt:unstructured"
                                        text="Montage - Asset Viewer"
                                        value="montage.assetviewer"/>
                                    <montageLightbox
                                        jcr:primaryType="nt:unstructured"
                                        text="Montage - Light Box"
                                        value="montage.lightbox"/>
                                </items>
                            </variant>
                        </items>
                    </column>
                </items>
            </layout>
            <styleTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Style"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <componentId
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="ID"
                                name="./componentId"
                                required="{Boolean}false"/>
                            <componentTheme
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Theme"
                                multiple="{Boolean}true"
                                name="./componentTheme"/>
                            <componentModifiers
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Modifiers"
                                multiple="{Boolean}true"
                                name="./componentModifiers"/>
                            <componentModule
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Module"
                                multiple="{Boolean}true"
                                name="./componentModule"/>
                            <componentChevron
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Chevron"
                                multiple="{Boolean}true"
                                name="./componentChevron"/>
                            <componentIcon
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Icon"
                                multiple="{Boolean}true"
                                name="./componentIcon"/>
                            <componentBooleanAttrs
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                                fieldLabel="Boolean Attributes"
                                multiple="{Boolean}true"
                                name="./componentBooleanAttrs"/>
                            <position
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Position coordinates"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <positions
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/sizefield"
                                        fieldDescription="The X and Y values of the Position"
                                        fieldLabel="Positions">
                                        <width
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                            name="./positionX"
                                            unit="X position"/>
                                        <height
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                            name="./positionY"
                                            unit="Y position"/>
                                    </positions>
                                </items>
                            </position>
                            <animation
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Animation"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <animationConfigGroupLabel/>
                                    <animationEnabled
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="Should the animation be enabled for this component?"
                                        fieldLabel="Enable animations"
                                        name="./animationEnabled"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationEnabled>
                                    <animationName
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="The type of animation to run"
                                        fieldLabel="Name"
                                        name="./animationName"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationName>
                                    <animationOnce
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="Should the animation only run once? By default is runs everytime you scroll past the element on the page"
                                        fieldLabel="Run Once?"
                                        name="./animationOnce"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationOnce>
                                    <animationEasing
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        fieldDescription="How should the animation play?"
                                        fieldLabel="Easing"
                                        name="./animationEasing"
                                        value="">
                                        <items jcr:primaryType="nt:unstructured"/>
                                    </animationEasing>
                                    <animationDelay
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        defaultValue=""
                                        fieldDescription="How many milliseconds should occur before the animation runs?"
                                        fieldLabel="Delay"
                                        name="./animationDelay"
                                        required="{Boolean}false"
                                        step="100"/>
                                    <animationDuration
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                        defaultValue=""
                                        fieldDescription="How long should the animation run for? Minimum is '400' and the maximum is '3000'. Values must be in increments of 50, i.e. 450, 500, 550..."
                                        fieldLabel="Duration"
                                        name="./animationDuration"
                                        required="{Boolean}false"
                                        step="50"/>
                                </items>
                            </animation>
                        </items>
                    </column>
                </items>
            </styleTab>
            <accessibilityTab
                jcr:primaryType="nt:unstructured"
                jcr:title="Accessibility"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <ariaLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Defines a string value that labels the current element."
                                fieldLabel="Aria Label"
                                name="./ariaLabel"
                                required="{Boolean}false"/>
                            <ariaRole
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Main indicator of type. This semantic association allows tools to present and support interaction with the object in a manner that is consistent with user expectations about other objects of that type."
                                fieldLabel="Aria Role"
                                name="./ariaRole"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaRole>
                            <ariaDescribedBy
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) that describes the object."
                                fieldLabel="Aria Described By"
                                name="./ariaDescribedBy"
                                required="{Boolean}false"/>
                            <ariaLabelledBy
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) that labels the current element."
                                fieldLabel="Aria Labelled By"
                                name="./ariaLabelledBy"
                                required="{Boolean}false"/>
                            <ariaControls
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) whose contents or presence are controlled by the current element. "
                                fieldLabel="Aria Controls"
                                name="./ariaControls"
                                required="{Boolean}false"/>
                            <ariaLive
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Indicates that an element will be updated, and describes the types of updates the user agents, assistive technologies, and user can expect from the live region."
                                fieldLabel="Aria Live"
                                name="./ariaLive"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaLive>
                            <ariaHidden
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Indicates that the element and all of its descendants are not visible or perceivable to any user as implemented by the author."
                                fieldLabel="Aria Hidden"
                                name="./ariaHidden"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaHidden>
                            <ariaHaspopup
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/select"
                                fieldDescription="Indicates that the element has a popup context menu or sub-level menu."
                                fieldLabel="Aria Has-Popup"
                                name="./ariaHaspopup"
                                value="">
                                <items jcr:primaryType="nt:unstructured"/>
                            </ariaHaspopup>
                            <ariaAccessKey
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldDescription="Identifies the element (or elements) whose contents or presence are controlled by the current element. "
                                fieldLabel="Access Key"
                                name="./ariaAccessKey"
                                required="{Boolean}false"/>
                        </items>
                    </column>
                </items>
            </accessibilityTab>
        </items>
    </content>
</jcr:root>
