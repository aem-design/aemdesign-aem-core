                                entity="&amp;darr;"/>
                            <trade
                                jcr:primaryType="cq:Tag"
                                sling:resourceType="cq/tagging/components/tag"
                                entity="&amp;trade;"/>
                            <blackdot
                                jcr:primaryType="cq:Tag"
                                sling:resourceType="cq/tagging/components/tag"
                                entity="●"/>
                            <cleardot
                                jcr:primaryType="cq:Tag"
                                sling:resourceType="cq/tagging/components/tag"
                                entity="○"/>
                            <charset
                                jcr:primaryType="cq:Tag"
                                sling:resourceType="cq/tagging/components/tag"
                                rangeEnd="190"
                                rangeStart="161"/>
                        </chars>
                    </specialCharsConfig>
                </misctools>
            </rtePlugins>
        </label>
        <linkUrl
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
            fieldLabel="Target URL"
            name="./linkUrl"
            required="{Boolean}false"/>
        <hrefTarget
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/form/select"
            fieldDescription="Where should the link be loaded when link is clicked"
            fieldLabel="Link Target"
            name="./linkTarget"
            value="">
            <items jcr:primaryType="nt:unstructured"/>
        </hrefTarget>
        <variant
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/form/select"
            fieldLabel="Variant"
            name="./variant"
            value="">
            <items jcr:primaryType="nt:unstructured">
                <default
                    jcr:primaryType="nt:unstructured"
                    text="Default"
                    value=""/>
                <button
                    jcr:primaryType="nt:unstructured"
                    text="Button"
                    value="button"/>
            </items>
        </variant>
    </items>
</jcr:root>
